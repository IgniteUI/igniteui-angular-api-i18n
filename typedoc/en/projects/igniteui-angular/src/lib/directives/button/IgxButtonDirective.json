{
    "IgxButtonDirective": {
        "comment": {
            "blockTags": [
                {
                    "tag": "@igxModule",
                    "content": [
                        {
                            "kind": "text",
                            "text": "IgxButtonModule"
                        }
                    ]
                },
                {
                    "tag": "@igxParent",
                    "content": [
                        {
                            "kind": "text",
                            "text": "Data Entry & Display"
                        }
                    ]
                },
                {
                    "tag": "@igxTheme",
                    "content": [
                        {
                            "kind": "text",
                            "text": "igx-button-theme"
                        }
                    ]
                },
                {
                    "tag": "@igxKeywords",
                    "content": [
                        {
                            "kind": "text",
                            "text": "button, span, div, click"
                        }
                    ]
                },
                {
                    "tag": "@remarks",
                    "content": [
                        {
                            "kind": "text",
                            "text": "The Ignite UI Button directive is intended to be used by any button, span or div and turn it into a fully functional button."
                        }
                    ]
                },
                {
                    "tag": "@example",
                    "content": [
                        {
                            "kind": "code",
                            "text": "```html\n<button type=\"button\" igxButton=\"outlined\">A Button</button>\n```"
                        }
                    ]
                }
            ],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "The Button directive provides the Ignite UI Button functionality to every component that's intended to be used as a button."
                }
            ]
        },
        "properties": {
            "buttonClick": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Called when the button is clicked."
                        }
                    ]
                }
            },
            "buttonSelected": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Called when the button is selected."
                        }
                    ]
                }
            },
            "role": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```typescript\nthis.button.role = 'navbutton';\nlet buttonRole = this.button.role;\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets/gets the "
                        },
                        {
                            "kind": "code",
                            "text": "`role`"
                        },
                        {
                            "kind": "text",
                            "text": " attribute."
                        }
                    ]
                }
            }
        },
        "methods": {},
        "accessors": {
            "selected": {
                "setter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<button type=\"button\" igxButton=\"flat\" [selected]=\"button.selected\"></button>\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Gets or sets whether the button is selected.\nMainly used in the IgxButtonGroup component and it will have no effect if set separately."
                            }
                        ]
                    }
                }
            },
            "nativeElement": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Returns the underlying DOM element."
                            }
                        ]
                    }
                }
            },
            "type": {
                "setter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<button type=\"button\" igxButton=\"icon\"></button>\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the type of the button."
                            }
                        ]
                    }
                }
            },
            "color": {
                "setter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<button type=\"button\" igxButton igxButtonColor=\"orange\"></button>\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the button text color."
                            }
                        ]
                    }
                }
            },
            "background": {
                "setter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<button type=\"button\" igxButton igxButtonBackground=\"red\"></button>\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the background color of the button."
                            }
                        ]
                    }
                }
            },
            "label": {
                "setter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<button type=\"button\" igxButton=\"flat\" igxLabel=\"Label\"></button>\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the "
                            },
                            {
                                "kind": "code",
                                "text": "`aria-label`"
                            },
                            {
                                "kind": "text",
                                "text": " attribute."
                            }
                        ]
                    }
                }
            },
            "disabled": {
                "getter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```typescript\nconst disabled = this.button.disabled;\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Get the disabled state of the button;"
                            }
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<button type=\"button\" igxButton=\"fab\" [disabled]=\"true\"></button>\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Enables/disables the button."
                            }
                        ]
                    }
                }
            },
            "displayDensity": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Returns the theme of the component.\nThe default theme is "
                            },
                            {
                                "kind": "code",
                                "text": "`comfortable`"
                            },
                            {
                                "kind": "text",
                                "text": ".\nAvailable options are "
                            },
                            {
                                "kind": "code",
                                "text": "`comfortable`"
                            },
                            {
                                "kind": "text",
                                "text": ", "
                            },
                            {
                                "kind": "code",
                                "text": "`cosy`"
                            },
                            {
                                "kind": "text",
                                "text": ", "
                            },
                            {
                                "kind": "code",
                                "text": "`compact`"
                            },
                            {
                                "kind": "text",
                                "text": ".\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\nlet componentTheme = this.component.displayDensity;\n```"
                            }
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the theme of the component."
                            }
                        ]
                    }
                }
            }
        }
    }
}
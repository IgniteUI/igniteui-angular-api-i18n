        "description": [
            "<p>Add theme colors as CSS variables to a given scope.</p>",
            ""
        ],
        "parameters": {
            "theme": {
                "description": [
                    "<p>The component theme to be used.</p>",
                    ""
                ]
            },
            "scope": {
                "description": [
                    "<p>The CSS variables scope to be used. (optional)</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p>Convert grid theme colors to css variables</p>",
                    ""
                ],
                "code": [
                    "$my-grid-theme grid-theme(",
                    "  $header-background: red,",
                    "  $content-background: #222",
                    ");",
                    ".my-grid {",
                    "  @include css-vars($my-grid-theme);",
                    "}"
                ]
            }
        ]
    },

                "comment": {
                    "blockTags": [
                        {
                            "tag": "@Input",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "property that allows you to disable the "
                                },
                                {
                                    "kind": "code",
                                    "text": "`active`"
                                },
                                {
                                    "kind": "text",
                                    "text": " property. By default it's applied."
                                }
                            ]
                        },
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-icon [active]=\"false\">settings</igx-icon>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "An"

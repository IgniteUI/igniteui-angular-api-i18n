                "```typescript",
                "@ViewChild('grid', { read: IgxGridComponent })",
                " public grid: IgxGridComponent;",
                "```",
                "```typescript",
                " let column = this.grid.columnList.first;",
                "```",
                "```typescript",
                " let cell = column.cells[0];",
                "```"

{
    "IgxListComponent": {
        "comment": {
            "blockTags": [
                {
                    "tag": "@igxModule",
                    "content": [
                        {
                            "kind": "text",
                            "text": "IgxListModule"
                        }
                    ]
                },
                {
                    "tag": "@igxTheme",
                    "content": [
                        {
                            "kind": "text",
                            "text": "igx-list-theme"
                        }
                    ]
                },
                {
                    "tag": "@igxKeywords",
                    "content": [
                        {
                            "kind": "text",
                            "text": "list, data"
                        }
                    ]
                },
                {
                    "tag": "@igxGroup",
                    "content": [
                        {
                            "kind": "text",
                            "text": "Grids & Lists"
                        }
                    ]
                },
                {
                    "tag": "@備考",
                    "content": [
                        {
                            "kind": "text",
                            "text": "The Ignite UI List displays rows of items and supports one or more header items as well as search and filtering\nof list items. Each list item is completely templatable and will support any valid HTML or Angular component."
                        }
                    ]
                },
                {
                    "tag": "@例",
                    "content": [
                        {
                            "kind": "code",
                            "text": "```html\n<igx-list>\n  <igx-list-item isHeader=\"true\">Contacts</igx-list-item>\n  <igx-list-item *ngFor=\"let contact of contacts\">\n    <span class=\"name\">{{ contact.name }}</span>\n    <span class=\"phone\">{{ contact.phone }}</span>\n  </igx-list-item>\n</igx-list>\n```"
                        }
                    ]
                }
            ],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "Displays a collection of data items in a templatable list format"
                }
            ]
        },
        "properties": {
            "children": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```typescript\nlet listChildren: QueryList = this.list.children;\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Returns a collection of all items and headers in the list."
                        }
                    ]
                }
            },
            "emptyListTemplate": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@備考",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "This template is used by IgxList in case there are no list items\ndefined and "
                                },
                                {
                                    "kind": "code",
                                    "text": "`isLoading`"
                                },
                                {
                                    "kind": "text",
                                    "text": " is set to "
                                },
                                {
                                    "kind": "code",
                                    "text": "`false`"
                                },
                                {
                                    "kind": "text",
                                    "text": "。"
                                }
                            ]
                        },
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-list>\n  <ng-template igxEmptyList>\n    <p class=\"empty\">No contacts! :(</p>\n  </ng-template>\n</igx-list>\n```"
                                },
                                {
                                    "kind": "text",
                                    "text": "\n"
                                },
                                {
                                    "kind": "code",
                                    "text": "```typescript\nlet emptyTemplate = this.list.emptyListTemplate;\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets/gets the empty list template."
                        }
                    ]
                }
            },

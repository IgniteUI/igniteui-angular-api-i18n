{
    "IgxStepComponent": {
        "comment": {
            "blockTags": [
                {
                    "tag": "@igxModule",
                    "content": [
                        {
                            "kind": "text",
                            "text": "IgxStepperModule"
                        }
                    ]
                },
                {
                    "tag": "@igxKeywords",
                    "content": [
                        {
                            "kind": "text",
                            "text": "step"
                        }
                    ]
                },
                {
                    "tag": "@例",
                    "content": [
                        {
                            "kind": "code",
                            "text": "```html\n <igx-stepper>\n ...\n   <igx-step [active]=\"true\" [completed]=\"true\">\n     ...\n   </igx-step>\n ...\n </igx-stepper>\n```"
                        }
                    ]
                }
            ],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "The IgxStepComponent is used within the "
                },
                {
                    "kind": "code",
                    "text": "`igx-stepper`"
                },
                {
                    "kind": "text",
                    "text": " element and it holds the content of each step.\nIt also supports custom indicators, title and subtitle."
                }
            ]
        },
        "properties": {
            "id": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Get/Set the "
                        },
                        {
                            "kind": "code",
                            "text": "`id`"
                        },
                        {
                            "kind": "text",
                            "text": " of the step component.\nデフォルト値は "
                        },
                        {
                            "kind": "code",
                            "text": "`\"igx-step-0\"`"
                        },
                        {
                            "kind": "text",
                            "text": ";\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-step id=\"my-first-step\"></igx-step>\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\nconst stepId = this.step.id;\n```"
                        }
                    ]
                }
            },
            "completed": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@備考",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "`true` "
                                },
                                {
                                    "kind": "code",
                                    "text": "に設定すると、"
                                },
                                {
                                    "kind": "text",
                                    "text": " the following separator is styled "
                                },
                                {
                                    "kind": "code",
                                    "text": "`solid`"
                                },
                                {
                                    "kind": "text",
                                    "text": ".\n\n"
                                },
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-stepper>\n...\n    <igx-step [completed]=\"true\"></igx-step>\n...\n</igx-stepper>\n```"
                                },
                                {
                                    "kind": "text",
                                    "text": "\n\n"
                                },
                                {
                                    "kind": "code",
                                    "text": "```typescript\nthis.stepper.steps[1].completed = true;\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Get/Set whether the step is completed."
                        }
                    ]
                }
            },
            "optional": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@備考",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "Optional steps validity does not affect the default behavior when the stepper is in linear mode i.e.\nif optional step is invalid the user could still move to the next step.\n\n"
                                },
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-step [optional]=\"true\"></igx-step>\n```"
                                },
                                {
                                    "kind": "text",
                                    "text": "\n"
                                },
                                {
                                    "kind": "code",
                                    "text": "```typescript\nthis.stepper.steps[1].optional = true;\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Get/Set whether the step is optional."
                        }
                    ]
                }
            },
            "activeChange": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emitted when the step's "
                        },
                        {
                            "kind": "code",
                            "text": "`active`"
                        },
                        {
                            "kind": "text",
                            "text": " property changes. Can be used for two-way binding.\n\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-step [(active)]=\"this.isActive\">\n</igx-step>\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\nconst step: IgxStepComponent = this.stepper.step[0];\nstep.activeChange.subscribe((e: boolean) => console.log(\"Step active state change to \", e))\n```"
                        }
                    ]
                }
            }
        },
        "methods": {},
        "accessors": {
            "disabled": {
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Get/Set whether the step is interactable.\n\n"
                            },
                            {
                                "kind": "code",
                                "text": "```html\n<igx-stepper>\n...\n    <igx-step [disabled]=\"true\"></igx-step>\n...\n</igx-stepper>\n```"
                            },
                            {
                                "kind": "text",
                                "text": "\n\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\nthis.stepper.steps[1].disabled = true;\n```"
                            }
                        ]
                    }
                }
            },
            "isValid": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Get/Set whether the step is valid.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```html\n<igx-step [isValid]=\"form.form.valid\">\n     ...\n     <div igxStepContent>\n         <form #form=\"ngForm\">\n             ...\n         </form>\n     </div>\n</igx-step>\n```"
                            }
                        ]
                    }
                }
            },
            "active": {
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Get/Set the active state of the step\n\n"
                            },
                            {
                                "kind": "code",
                                "text": "```html\n<igx-step [active]=\"true\"></igx-step>\n```"
                            },
                            {
                                "kind": "text",
                                "text": "\n\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\nthis.stepper.steps[1].active = true;\n```"
                            }
                        ]
                    }
                }
            },
            "index": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Get the step index inside of the stepper.\n\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\nconst step = this.stepper.steps[1];\nconst stepIndex: number = step.index;\n```"
                            }
                        ]
                    }
                }
            }
        }
    }
}
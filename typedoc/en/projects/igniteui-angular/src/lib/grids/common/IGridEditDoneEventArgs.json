{
    "IGridEditDoneEventArgs": {
        "comment": {
            "blockTags": [],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "Represents event arguments related to grid editing completion."
                }
            ]
        },
        "properties": {
            "cellID": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "code",
                            "text": "`cellID`"
                        },
                        {
                            "kind": "text",
                            "text": " is optional; specifies the cell the editing is being done on."
                        }
                    ]
                }
            },
            "rowData": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "code",
                            "text": "`rowData`"
                        },
                        {
                            "kind": "text",
                            "text": " represents the updated/committed data of the row after the edit (newValue)\nThe only case rowData (of the current object) is used directly, is when there is no rowEditing or transactions enabled"
                        }
                    ]
                }
            },
            "oldValue": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Represents the previous (before editing) value of the edited cell. \nIt's used when the event has been stoped/exited."
                        }
                    ]
                }
            },
            "newValue": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Optional\nRepresents the value, that is being entered in the edited cell\nWhen there is no "
                        },
                        {
                            "kind": "code",
                            "text": "`newValue`"
                        },
                        {
                            "kind": "text",
                            "text": " and the event has ended, the value of the cell returns to the "
                        },
                        {
                            "kind": "code",
                            "text": "`oldValue`"
                        }
                    ]
                }
            },
            "event": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Optional\nRepresents the original event, that has triggered the edit"
                        }
                    ]
                }
            },
            "column": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Optional\nRepresents the column information of the edited cell"
                        }
                    ]
                }
            },
            "owner": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Optional\nRepresents the grid instance that owns the edit event."
                        }
                    ]
                }
            },
            "isAddRow": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Optional\nIndicates if the editing cosists of adding a new row"
                        }
                    ]
                }
            },
            "valid": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Optional\nIndicates if the new value would be valid. \nIt can be set to return the result of the methods for validation of the grid"
                        }
                    ]
                }
            }
        },
        "methods": {}
    }
}
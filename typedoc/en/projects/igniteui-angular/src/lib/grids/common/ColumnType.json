                "comment": {
                    "blockTags": [
                        {
                            "tag": "@deprecated",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "in version 13.1.0. Use "
                                },
                                {
                                    "kind": "code",
                                    "text": "`IgxGridComponent.moving`"
                                },
                                {
                                    "kind": "text",
                                    "text": " instead."
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": []
                }
            },

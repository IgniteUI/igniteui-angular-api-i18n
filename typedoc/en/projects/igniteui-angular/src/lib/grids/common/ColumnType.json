                        }
                    ]
                }
            },
            "headerStyles": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Represents custom CSS styles applied to the header element. When added, they take different styling"
                        }
                    ]
                }
            },
            "headerGroupClasses": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Represents custom CSS classes applied to the header group. When added, they take different styling"
                        }
                    ]
                }
            },
            "headerGroupStyles": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Represents custom CSS styles applied to the header group. When added, they take different styling"

                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n@ViewChild('rippleContainer', {read: IgxRippleDirective})\npublic ripple: IgxRippleDirective;\nlet rippleColor = this.ripple.rippleColor;\n```"
                        }
                    ]

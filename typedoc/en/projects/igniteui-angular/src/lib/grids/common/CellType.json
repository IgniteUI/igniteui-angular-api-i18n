                    ]
                }
            },
            "width": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "The CSS width of the cell as a string."
                        }
                    ]
                }
            },
            "visibleColumnIndex": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",

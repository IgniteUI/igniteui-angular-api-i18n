{
    "elevations": {
        "description": [
            "<p>Generates CSS variables for a given elevations map.</p>",
            ""
        ],
        "parameters": {
            "elevations": {
                "description": [
                    "<p>The elevations map to use to generate CSS variables.</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p>Generate CSS variables for elevations.</p>",
                    ""
                ],
                "code": [
                    "$elevations: (",
                    "    small: box-shadow(0 .125rem .25rem rgba(0 0 0 / 75%)),",
                    "    medium: box-shadow(0 .25rem .5rem rgba(0 0 0 / 85%)),",
                    "    large: box-shadow(0 .75rem 1rem rgba(0 0 0 / 95%))",
                    ");",
                    ":root {",
                    "    @include elevations($elevations);",
                    "}"
                ]
            }
        ]
    },
    "elevation": {
        "description": [
            "<p>Includes box-shadow styles for an elevation by name</p>",
            ""
        ],
        "parameters": {
            "name": {
                "description": [
                    "<p>The name of the shadow.</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p>Include a box shadow by its name.</p>",
                    ""
                ],
                "code": [
                    ".my-class {",
                    "  @include elevation(small);",
                    "}"
                ]
            }
        ]
    }
}
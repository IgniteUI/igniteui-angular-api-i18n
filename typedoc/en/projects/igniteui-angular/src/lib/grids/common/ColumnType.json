{
    "ColumnType": {
        "comment": {
            "blockTags": [],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "Represents a column in the "
                },
                {
                    "kind": "code",
                    "text": "`GridType`"
                },
                {
                    "kind": "text",
                    "text": ". It is essentially the blueprint to a column object.\nContains definitions of properties and methods, relevant to a column"
                }
            ]
        },
        "properties": {
            "grid": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Represents the inctance of the parent "
                        },
                        {
                            "kind": "code",
                            "text": "`GridType`"
                        },
                        {
                            "kind": "text",
                            "text": " that contains this column."
                        }
                    ]
                }
            },
            "children": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",

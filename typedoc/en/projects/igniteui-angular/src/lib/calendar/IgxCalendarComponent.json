{
    "IgxCalendarComponent": {
        "comment": {
            "blockTags": [
                {
                    "tag": "@igxModule",
                    "content": [
                        {
                            "kind": "text",
                            "text": "IgxCalendarModule"
                        }
                    ]
                },
                {
                    "tag": "@igxTheme",
                    "content": [
                        {
                            "kind": "text",
                            "text": "igx-calendar-theme, igx-icon-theme"
                        }
                    ]
                },
                {
                    "tag": "@igxKeywords",
                    "content": [
                        {
                            "kind": "text",
                            "text": "calendar, datepicker, schedule, date"
                        }
                    ]
                },
                {
                    "tag": "@igxGroup",
                    "content": [
                        {
                            "kind": "text",
                            "text": "Scheduling"
                        }
                    ]
                },
                {
                    "tag": "@remarks",
                    "content": [
                        {
                            "kind": "text",
                            "text": "The Ignite UI Calendar provides an easy way to display a calendar and allow users to select dates using single, multiple\nor range selection.\n\n@example:\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-calendar selection=\"range\"></igx-calendar>\n```"
                        }
                    ]
                }
            ],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "Calendar provides a way to display date information."
                }
            ]
        },
        "properties": {
            "id": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@remarks",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "If not set, the "
                                },
                                {
                                    "kind": "code",
                                    "text": "`id`"
                                },
                                {
                                    "kind": "text",
                                    "text": " will have value "
                                },
                                {
                                    "kind": "code",
                                    "text": "`\"igx-calendar-0\"`"
                                },
                                {
                                    "kind": "text",
                                    "text": "."
                                }
                            ]
                        },
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-calendar id=\"my-first-calendar\"></igx-calendar>\n```"
                                }
                            ]
                        },
                        {
                            "tag": "@memberof",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "IgxCalendarComponent"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets/gets the "
                        },
                        {
                            "kind": "code",
                            "text": "`id`"
                        },
                        {
                            "kind": "text",
                            "text": " of the calendar."
                        }
                    ]
                }
            },
            "hasHeader": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-calendar [hasHeader]=\"false\"></igx-calendar>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets/gets whether the calendar has header.\nDefault value is "
                        },
                        {
                            "kind": "code",
                            "text": "`true`"
                        },
                        {
                            "kind": "text",
                            "text": "."
                        }
                    ]
                }
            },
            "vertical": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-calendar [vertical] = \"true\"></igx-calendar>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets/gets whether the calendar header will be in vertical position.\nDefault value is "
                        },
                        {
                            "kind": "code",
                            "text": "`false`"
                        },
                        {
                            "kind": "text",
                            "text": "."
                        }
                    ]
                }
            },
            "showWeekNumbers": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-calendar [showWeekNumbers]=\"true\"></igx-calendar>\n``"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Show/hide week numbers"
                        }
                    ]
                }
            },
            "hideOutsideDays": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets/gets whether the outside dates (dates that are out of the current month) will be hidden.\nDefault value is "
                        },
                        {
                            "kind": "code",
                            "text": "`false`"
                        },
                        {
                            "kind": "text",
                            "text": ".\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-calendar [hideOutsideDays] = \"true\"></igx-calendar>\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\nlet hideOutsideDays = this.calendar.hideOutsideDays;\n```"
                        }
                    ]
                }
            },
            "selected": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emits an event when a date is selected.\nProvides reference the "
                        },
                        {
                            "kind": "code",
                            "text": "`selectedDates`"
                        },
                        {
                            "kind": "text",
                            "text": " property."
                        }
                    ]
                }
            },
            "viewDateChanged": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emits an event when the month in view is changed.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-calendar (viewDateChanged)=\"viewDateChanged($event)\"></igx-calendar>\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\npublic viewDateChanged(event: IViewDateChangeEventArgs) {\n let viewDate = event.currentValue;\n}\n```"
                        }
                    ]
                }
            },
            "activeViewChanged": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emits an event when the active view is changed.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-calendar (activeViewChanged)=\"activeViewChanged($event)\"></igx-calendar>\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\npublic activeViewChanged(event: CalendarView) {\n let activeView = event;\n}\n```"
                        }
                    ]
                }
            }
        },
        "methods": {
            "deselectDate": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```typescript\n this.calendar.deselectDate(new Date(`2018-06-12`));\n````"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Deselects date(s) (based on the selection type)."
                        }
                    ]
                }
            },
            "selectDate": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Selects date(s) (based on the selection type)."
                        }
                    ]
                }
            }
        },

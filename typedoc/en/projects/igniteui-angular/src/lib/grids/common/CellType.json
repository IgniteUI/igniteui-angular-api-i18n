{
    "CellType": {
        "comment": {
            "blockTags": [],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "Interface representing a cell in the grid. It is essentially the blueprint to a cell object.\nContains definitions of properties and methods, relevant to a cell"
                }
            ]
        },
        "properties": {
            "value": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "The current value of the cell."
                        }
                    ]
                }
            },
            "editValue": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "The value to display when the cell is in edit mode."
                        }
                    ]
                }
            },
            "selected": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Indicates whether the cell is currently selected. It is false, if the sell is not selected, and true, if it is."
                        }
                    ]
                }
            },
            "active": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Indicates whether the cell is currently active (focused)."
                        }
                    ]
                }
            },
            "editable": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Indicates whether the cell can be edited."
                        }
                    ]
                }
            },
            "editMode": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Indicates whether the cell is currently in edit mode."
                        }
                    ]
                }
            },
            "nativeElement": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Represents the native HTML element of the cell itself"

{
    "IgxIconService": {
        "comment": {
            "blockTags": [],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "**Ignite UI for Angular Icon Service** -\n\nThe Ignite UI Icon Service makes it easy for developers to include custom SVG images and use them with IgxIconComponent.\nIn addition it could be used to associate a custom class to be applied on IgxIconComponent according to given font-family.\n\nExample:\n"
                },
                {
                    "kind": "code",
                    "text": "```typescript\nthis.iconService.registerFamilyAlias('material', 'material-icons');\nthis.iconService.addSvgIcon('aruba', '/assets/svg/country_flags/aruba.svg', 'svg-flags');\n```"
                }
            ]
        },
        "properties": {
            "iconLoaded": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```typescript\nthis.service.iconLoaded.subscribe((ev: IgxIconLoadedEvent) => ...);\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Observable that emits when an icon is successfully loaded\nthrough a HTTP request."
                        }
                    ]
                }
            }
        },
        "methods": {
            "registerFamilyAlias": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Registers a custom class to be applied to IgxIconComponent for a given font-family.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n  this.iconService.registerFamilyAlias('material', 'material-icons');\n```"
                        }
                    ]
                }
            },
            "familyClassName": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Returns the custom class, if any, associated to a given font-family.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n  const familyClass = this.iconService.familyClassName('material');\n```"
                        }
                    ]
                }
            },
            "addSvgIcon": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Adds an SVG image to the cache. SVG source is an url.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n  this.iconService.addSvgIcon('aruba', '/assets/svg/country_flags/aruba.svg', 'svg-flags');\n```"
                        }
                    ]
                }
            },
            "addSvgIconFromText": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Adds an SVG image to the cache. SVG source is its text.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n  this.iconService.addSvgIconFromText('simple', '<svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 200 200\">\n  <path d=\"M74 74h54v54H74\" /></svg>', 'svg-flags');\n```"
                        }
                    ]
                }
            },
            "isSvgIconCached": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Returns whether a given SVG image is present in the cache.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n  const isSvgCached = this.iconService.isSvgIconCached('aruba', 'svg-flags');\n```"
                        }
                    ]
                }
            },
            "getSvgIcon": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Returns the cached SVG image as string.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n  const svgIcon = this.iconService.getSvgIcon('aruba', 'svg-flags');\n```"
                        }
                    ]
                }
            }
        },
        "accessors": {
            "defaultFamily": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Returns the default font-family.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\n  const defaultFamily = this.iconService.defaultFamily;\n```"
                            }
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the default font-family.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\n  this.iconService.defaultFamily = 'svg-flags';\n```"
                            }
                        ]
                    }
                }
            }
        }
    }
}
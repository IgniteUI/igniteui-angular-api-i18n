                    ]
                }
            },
            "cellID": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Optional; The "
                        },
                        {
                            "kind": "code",
                            "text": "`cellID`"
                        },
                        {
                            "kind": "text",
                            "text": " is the unique key, used to identify the cell"
                        }
                    ]
                }
            },
            "validation": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",

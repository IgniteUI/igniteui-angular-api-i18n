{
    "box-shadow": {
        "description": [
            "<p>Sorts out a list of valid only box-shadows.</p>",
            ""
        ],
        "parameters": {
            "shadows": {
                "description": [
                    "<p>A list of shadow values.</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>The transformed shadow list.</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "box-shadow((0 0 2px 3px black, 0 6px 9px orange));",
                    "// Return",
                    "box-shadow:",
                    "   0 0 calc(--ig-elevation-factor * 2px) calc(--ig-elevation-factor * 3px) black,",
                    "   0 calc(--ig-elevation-factor * 6px) calc(--ig-elevation-factor * 9px) orange;"
                ]
            }
        ]
    },
    "elevation": {
        "description": [
            "<p>Gets a CSS elevation variable by name.</p>",
            ""
        ],
        "parameters": {
            "name": {
                "description": [
                    "<p>The name of the shadow.</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>The CSS elevation variable</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    ".my-class {",
                    "  box-shadow: elevation(5);",
                    "}"
                ]
            }
        ]
    }
}
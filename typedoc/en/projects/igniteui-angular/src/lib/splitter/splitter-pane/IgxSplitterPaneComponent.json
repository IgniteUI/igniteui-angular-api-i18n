                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-splitter>\n <igx-splitter-pane [resizable]='false'>...</igx-splitter-pane>\n</igx-splitter>\n```"
                                }
                            ]
                        },
                        {
                            "tag": "@remarks",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "If pane is not resizable its related splitter bar cannot be dragged."
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Gets/Sets whether pane is resizable."

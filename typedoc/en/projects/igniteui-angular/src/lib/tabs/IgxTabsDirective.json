{
    "IgxTabsDirective": {
        "properties": {
            "selectedIndexChange": {
                "comment": {
                    "shortText": "Output to enable support for two-way binding on [(selectedIndex)]"
                }
            },
            "selectedIndexChanging": {
                "comment": {
                    "shortText": "Emitted when the selected index is about to change."
                }
            },
            "selectedItemChange": {
                "comment": {
                    "shortText": "Emitted when the selected item is changed."
                }
            },
            "items": {
                "comment": {
                    "shortText": "Returns the items."
                }
            }
        },
        "methods": {},
        "accessors": {
            "selectedIndex": {
                "getter": {
                    "comment": {
                        "shortText": [
                            "An @Input property that gets/sets the index of the selected item.",
                            "Default value is 0 if contents are defined otherwise defaults to -1."
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "shortText": [
                            "An @Input property that gets/sets the index of the selected item.",
                            "Default value is 0 if contents are defined otherwise defaults to -1."
                        ]
                    }
                }
            },
            "disableAnimation": {
                "getter": {
                    "comment": {
                        "shortText": "Enables/disables the transition animation of the contents."
                    }
                },
                "setter": {
                    "comment": {
                        "shortText": "Enables/disables the transition animation of the contents."
                    }
                }
            },
            "selectedItem": {
                "getter": {
                    "comment": {
                        "shortText": "Gets the selected item."
                    }
                }
            }
        }
    }
}
{
    "IgxDateTimeEditorDirective": {
        "comment": {
            "blockTags": [
                {
                    "tag": "@igxModule",
                    "content": [
                        {
                            "kind": "text",
                            "text": "IgxDateTimeEditorModule"
                        }
                    ]
                },
                {
                    "tag": "@igxParent",
                    "content": [
                        {
                            "kind": "text",
                            "text": "IgxInputGroup"
                        }
                    ]
                },
                {
                    "tag": "@igxTheme",
                    "content": [
                        {
                            "kind": "text",
                            "text": "igx-input-theme"
                        }
                    ]
                },
                {
                    "tag": "@igxKeywords",
                    "content": [
                        {
                            "kind": "text",
                            "text": "date, time, editor"
                        }
                    ]
                },
                {
                    "tag": "@igxGroup",
                    "content": [
                        {
                            "kind": "text",
                            "text": "Scheduling"
                        }
                    ]
                },
                {
                    "tag": "@備考",
                    "content": [
                        {
                            "kind": "text",
                            "text": "The Ignite UI Date Time Editor Directive makes it easy for developers to manipulate date/time user input.\nIt requires input in a specified or default input format which is visible in the input element as a placeholder.\nIt allows the input of only date (ex: 'dd/MM/yyyy'), only time (ex:'HH:mm tt') or both at once, if needed.\nSupports display format that may differ from the input format.\nProvides methods to increment and decrement any specific/targeted "
                        },
                        {
                            "kind": "code",
                            "text": "`DatePart`"
                        },
                        {
                            "kind": "text",
                            "text": "。"
                        }
                    ]
                },
                {
                    "tag": "@例",
                    "content": [
                        {
                            "kind": "code",
                            "text": "```html\n<igx-input-group>\n  <input type=\"text\" igxInput [igxDateTimeEditor]=\"'dd/MM/yyyy'\" [displayFormat]=\"'shortDate'\" [(ngModel)]=\"date\"/>\n</igx-input-group>\n```"
                        }
                    ]
                }
            ],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "Date Time Editor provides a functionality to input, edit and format date and time."
                }
            ]
        },
        "properties": {
            "locale": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@備考",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "Uses Angular's "
                                },
                                {
                                    "kind": "code",
                                    "text": "`LOCALE_ID`"
                                },
                                {
                                    "kind": "text",
                                    "text": " by default. Affects both input mask and display format if those are not set.\nIf a "
                                },
                                {
                                    "kind": "code",
                                    "text": "`locale`"
                                },
                                {
                                    "kind": "text",
                                    "text": " is set, it must be registered via "
                                },
                                {
                                    "kind": "code",
                                    "text": "`registerLocaleData`"
                                },
                                {
                                    "kind": "text",
                                    "text": ".\nPlease refer to https://angular.io/guide/i18n#i18n-pipes.\nIf it is not registered, "
                                },
                                {
                                    "kind": "code",
                                    "text": "`Intl`"
                                },
                                {
                                    "kind": "text",
                                    "text": " will be used for formatting."
                                }
                            ]
                        },
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<input igxDateTimeEditor [locale]=\"'en'\">\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Locale settings used for value formatting."
                        }
                    ]
                }
            },
            "spinLoop": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<input igxDateTimeEditor [spinLoop]=\"false\">\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Specify if the currently spun date segment should loop over."
                        }
                    ]
                }
            },
            "displayFormat": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<input igxDateTimeEditor [displayFormat]=\"'shortDate'\">\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Set both pre-defined format options such as "
                        },
                        {
                            "kind": "code",
                            "text": "`shortDate`"
                        },
                        {
                            "kind": "text",
                            "text": " and "
                        },
                        {
                            "kind": "code",
                            "text": "`longDate`"
                        },
                        {
                            "kind": "text",
                            "text": ",\nas well as constructed format string using characters supported by "
                        },
                        {
                            "kind": "code",
                            "text": "`DatePipe`"
                        },
                        {
                            "kind": "text",
                            "text": ", e.g. "
                        },
                        {
                            "kind": "code",
                            "text": "`EE/MM/yyyy`"
                        },
                        {
                            "kind": "text",
                            "text": "。"
                        }
                    ]
                }
            },
            "spinDelta": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<input igxDateTimeEditor [spinDelta]=\"{date: 5, minute: 30}\">\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Delta values used to increment or decrement each editor date part on spin actions.\nAll values default to "
                        },
                        {
                            "kind": "code",
                            "text": "`1`"
                        },
                        {
                            "kind": "text",
                            "text": "。"
                        }
                    ]
                }
            },
            "valueChange": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<input igxDateTimeEditor (valueChange)=\"valueChange($event)\"/>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emitted when the editor's value has changed."
                        }
                    ]
                }
            },
            "validationFailed": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<input igxDateTimeEditor [minValue]=\"minDate\" [maxValue]=\"maxDate\" (validationFailed)=\"onValidationFailed($event)\"/>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emitted when the editor is not within a specified range or when the editor's value is in an invalid state."
                        }
                    ]
                }
            },
            "promptChar": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets the character representing a fillable spot in the input mask.\nDefault value is \"'_'\".\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input [promptChar] = \"'/'\">\n```"
                        }
                    ]
                }
            },
            "includeLiterals": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Specifies if the bound value includes the formatting symbols.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input [includeLiterals] = \"true\">\n```"
                        }
                    ]
                }
            },
            "displayValuePipe": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Specifies a pipe to be used on blur.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input [displayValuePipe] = \"displayFormatPipe\">\n```"
                        }
                    ]
                }
            },
            "focusedValuePipe": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Specifies a pipe to be used on focus.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input [focusedValuePipe] = \"inputFormatPipe\">\n```"
                        }
                    ]
                }
            },
            "valueChanged": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emits an event each time the value changes.\nProvides "
                        },
                        {
                            "kind": "code",
                            "text": "`rawValue: string`"
                        },
                        {
                            "kind": "text",
                            "text": " and "
                        },
                        {
                            "kind": "code",
                            "text": "`formattedValue: string`"
                        },
                        {
                            "kind": "text",
                            "text": " as event arguments.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input (valueChanged) = \"valueChanged(rawValue: string, formattedValue: string)\">\n```"
                        }
                    ]
                }
            }
        },
        "methods": {
            "clear": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Clear the input element value."
                        }
                    ]
                }
            },
            "increment": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Increment specified DatePart."
                        }
                    ]
                }
            },
            "decrement": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Decrement specified DatePart."
                        }
                    ]
                }
            }
        },
        "accessors": {
            "minValue": {
                "getter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@備考",
                                "content": [
                                    {
                                        "kind": "text",
                                        "text": "If a "
                                    },
                                    {
                                        "kind": "code",
                                        "text": "`string`"
                                    },
                                    {
                                        "kind": "text",
                                        "text": " value is passed, it must be in the defined input format."
                                    }
                                ]
                            },
                            {
                                "tag": "@例",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<input igxDateTimeEditor [minValue]=\"minDate\">\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Minimum value required for the editor to remain valid."
                            }
                        ]
                    }
                }
            },
            "maxValue": {
                "getter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@備考",
                                "content": [
                                    {
                                        "kind": "text",
                                        "text": "If a "
                                    },
                                    {
                                        "kind": "code",
                                        "text": "`string`"
                                    },
                                    {
                                        "kind": "text",
                                        "text": " value is passed in, it must be in the defined input format."
                                    }
                                ]
                            },
                            {
                                "tag": "@例",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<input igxDateTimeEditor [maxValue]=\"maxDate\">\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Maximum value required for the editor to remain valid."
                            }
                        ]
                    }
                }
            },
            "inputFormat": {
                "setter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@例",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<input [igxDateTimeEditor]=\"'dd/MM/yyyy'\">\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Expected user input format (and placeholder)."
                            }
                        ]
                    }
                }
            },
            "value": {
                "setter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@例",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<input igxDateTimeEditor [value]=\"date\">\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Editor value."
                            }
                        ]
                    }
                }
            },
            "mask": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the input mask.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```html\n<input [igxMask] = \"'00/00/0000'\">\n```"
                            }
                        ]
                    }
                }
            }
        }
    }
}
                            {
                                "kind": "code",
                                "text": "`day`"
                            },
                            {
                                "kind": "text",
                                "text": ", "
                            },
                            {
                                "kind": "code",
                                "text": "`month`"
                            },
                            {
                                "kind": "text",
                                "text": " and "
                            },
                            {
                                "kind": "code",
                                "text": "`year`"
                            },
                            {
                                "kind": "text",
                                "text": " should be rendered\naccording to the locale and formatOptions, if any."
                            }
                        ]
                    }
                }
            },

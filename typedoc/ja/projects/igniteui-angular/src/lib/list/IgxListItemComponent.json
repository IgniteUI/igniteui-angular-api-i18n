                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Provides a reference to the template's base element shown when left panning a list item.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\nconst leftPanTmpl = this.listItem.leftPanningTemplateElement;\n```"
                        }
                    ]
                }
            },

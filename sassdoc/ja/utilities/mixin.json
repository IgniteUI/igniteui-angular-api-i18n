{
    "hide-default": {
        "description": [
            "<p>DOM から要素を非表示にします。</p>",
            ""
        ],
        "example": [
            {
                "description": [
                    "<p>例</p>",
                    ""
                ],
                "code": [
                    "input[type=\"checkbox\"] {",
                    "  @include hide-default();",
                    "}"
                ]
            }
        ]

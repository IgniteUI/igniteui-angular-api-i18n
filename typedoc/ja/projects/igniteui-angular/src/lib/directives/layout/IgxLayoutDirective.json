{
    "IgxLayoutDirective": {
        "properties": {
            "dir": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets the default flow direction of the container's children.\r\n\r\nDefaults to "
                        },
                        {
                            "kind": "code",
                            "text": "`rows`"
                        },
                        {
                            "kind": "text",
                            "text": ".\r\n\r\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\r\n <div\r\n  igxLayout\r\n  igxLayoutDir=\"row\">\r\n   <div igxFlex>1</div>\r\n   <div igxFlex>2</div>\r\n   <div igxFlex>3</div>\r\n </div>\r\n```"
                        }
                    ]
                }
            },
            "reverse": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Defines the direction flex children are placed in the flex container.\r\n\r\nWhen set to "
                        },
                        {
                            "kind": "code",
                            "text": "`true`"
                        },
                        {
                            "kind": "text",
                            "text": ", the "
                        },
                        {
                            "kind": "code",
                            "text": "`rows`"
                        },
                        {
                            "kind": "text",
                            "text": " direction goes right to left and "
                        },
                        {
                            "kind": "code",
                            "text": "`columns`"
                        },
                        {
                            "kind": "text",
                            "text": " goes bottom to top.\r\n\r\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\r\n<div\r\n  igxLayout\r\n  igxLayoutReverse=\"true\">\r\n   <div igxFlex>1</div>\r\n   <div igxFlex>2</div>\r\n   <div igxFlex>3</div>\r\n</div>\r\n```"
                        }
                    ]
                }
            },
            "wrap": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "By default the immediate children will all try to fit onto one line.\r\n\r\nThe default value "
                        },
                        {
                            "kind": "code",
                            "text": "`nowrap`"
                        },
                        {
                            "kind": "text",
                            "text": " sets this behavior.\r\n\r\nOther accepted values are "
                        },
                        {
                            "kind": "code",
                            "text": "`wrap`"
                        },
                        {
                            "kind": "text",
                            "text": " and "
                        },
                        {
                            "kind": "code",
                            "text": "`wrap-reverse`"
                        },
                        {
                            "kind": "text",
                            "text": ".\r\n\r\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\r\n<div\r\n  igxLayout\r\n  igxLayoutDir=\"row\"\r\n  igxLayoutWrap=\"wrap\">\r\n   <div igxFlex igxFlexGrow=\"0\">1</div>\r\n   <div igxFlex igxFlexGrow=\"0\">2</div>\r\n   <div igxFlex igxFlexGrow=\"0\">3</div>\r\n</div>\r\n```"
                        }
                    ]
                }
            },
            "justify": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Defines the alignment along the main axis.\r\n\r\nDefaults to "
                        },
                        {
                            "kind": "code",
                            "text": "`flex-start`"
                        },
                        {
                            "kind": "text",
                            "text": " which packs the children toward the start line.\r\n\r\nOther possible values are "
                        },
                        {
                            "kind": "code",
                            "text": "`flex-end`"
                        },
                        {
                            "kind": "text",
                            "text": "、"
                        },
                        {
                            "kind": "code",
                            "text": "`center`"
                        },
                        {
                            "kind": "text",
                            "text": "、"
                        },
                        {
                            "kind": "code",
                            "text": "`space-between`"
                        },
                        {
                            "kind": "text",
                            "text": "、"
                        },
                        {
                            "kind": "code",
                            "text": "`space-around`"
                        },
                        {
                            "kind": "text",
                            "text": ".\r\n\r\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\r\n<div\r\n  igxLayout\r\n  igxLayoutDir=\"column\"\r\n  igxLayoutJustify=\"space-between\">\r\n   <div>1</div>\r\n   <div>2</div>\r\n   <div>3</div>\r\n</div>\r\n```"
                        }
                    ]
                }
            },
            "itemAlign": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Defines the default behavior for how children are laid out along the corss axis of the current line.\r\n\r\nDefaults to "
                        },
                        {
                            "kind": "code",
                            "text": "`flex-start`"
                        },
                        {
                            "kind": "text",
                            "text": ".\r\n\r\nOther possible values are "
                        },
                        {
                            "kind": "code",
                            "text": "`flex-end`"
                        },
                        {
                            "kind": "text",
                            "text": "、"
                        },
                        {
                            "kind": "code",
                            "text": "`center`"
                        },
                        {
                            "kind": "text",
                            "text": "、"
                        },
                        {
                            "kind": "code",
                            "text": "`baseline`"
                        },
                        {
                            "kind": "text",
                            "text": ", and "
                        },
                        {
                            "kind": "code",
                            "text": "`stretch`"
                        },
                        {
                            "kind": "text",
                            "text": ".\r\n\r\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\r\n<div\r\n  igxLayout\r\n  igxLayoutDir=\"column\"\r\n  igxLayoutItemAlign=\"start\">\r\n   <div igxFlex igxFlexGrow=\"0\">1</div>\r\n   <div igxFlex igxFlexGrow=\"0\">2</div>\r\n   <div igxFlex igxFlexGrow=\"0\">3</div>\r\n</div>\r\n```"
                        }
                    ]
                }
            }
        },
        "methods": {},
        "accessors": {}
    }
}
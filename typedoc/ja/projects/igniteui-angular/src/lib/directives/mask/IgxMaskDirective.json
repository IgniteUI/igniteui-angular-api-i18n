{
    "IgxMaskDirective": {
        "properties": {
            "promptChar": {
                "comment": {
                    "shortText": [
                        "入力マスクで入力可能な文字を表すキャラクターを設定します。",
                        "デフォルト値は \"'_'\" です。",
                        "```html",
                        "<input [promptChar] = \"'/'\">",
                        "```"
                    ]
                }
            },
            "includeLiterals": {
                "comment": {
                    "shortText": [
                        "バインドされた値が書式設定記号を含むかどうかを指定します。",
                        "```html",
                        "<input [includeLiterals] = \"true\">",
                        "```"
                    ]
                }
            },
            "displayValuePipe": {
                "comment": {
                    "shortText": [
                        "ぼかしに使用されるパイプを指定します。",
                        "```html",
                        "<input [displayValuePipe] = \"displayFormatPipe\">",
                        "```"
                    ]
                }
            },
            "focusedValuePipe": {
                "comment": {
                    "shortText": [
                        "フォーカスに使用される pipe を指定します。",
                        "```html",
                        "<input [displayValuePipe] = \"displayFormatPipe\">",
                        "```"
                    ]
                }
            },
            "onValueChange": {
                "comment": {
                    "shortText": [
                        "値が変更されたときにイベントを発生します。",
                        "イベント引数として `rawValue: string` および `formattedValue: string` を提供します。",
                        "```html",
                        "<input (onValueChange) = \"onValueChange(rawValue: string, formattedValue: string)\">",
                        "```"
                    ]
                }
            }
        },
        "methods": {},
        "accessors": {
            "mask": {
                "getter": {
                    "comment": {
                        "shortText": [
                            "入力マスクを設定します。",
                            "```html",
                            "<input [igxMask] = \"'00/00/0000'\">",
                            "```"
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "shortText": [
                            "入力マスクを設定します。",
                            "```html",
                            "<input [igxMask] = \"'00/00/0000'\">",
                            "```"
                        ]
                    }
                }
            },
            "placeholder": {
                "setter": {
                    "comment": {
                        "shortText": [
                            "プレースホルダを指定します。",
                            "```html",
                            "<input placeholder = \"enter text...\">",
                            "```"
                        ]
                    }
                },
                "getter": {
                    "comment": {
                        "shortText": [
                            "プレースホルダを指定します。",
                            "```html",
                            "<input placeholder = \"enter text...\">",
                            "```"
                        ]
                    }
                }
            }
        }
    }
}
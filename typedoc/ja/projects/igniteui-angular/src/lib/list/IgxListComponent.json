            },
            "headers": {
                "getter": {
                    "comment": {
                        "tags": {
                            "example": {
                                "comment": {
                                    "text": [
                                        "```typescript",
                                        "let listHeaders: IgxListItemComponent[] =  this.list.headers;",
                                        "```"
                                    ],
                                    "tagName": "例:"
                                }
                            }
                        },
                        "shortText": "ヘッダー リストの `items` を取得します。"
                    }

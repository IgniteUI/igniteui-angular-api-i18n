                            {
                                "kind": "code",
                                "text": "`igxCalendarSubHeaderYear`"
                            },
                            {
                                "kind": "text",
                                "text": " directive."
                            }
                        ]
                    }
                }
            },
            "activeView": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Gets the current active view.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\nthis.activeView = calendar.activeView;\n```"
                            }

                    }
                },
                "setter": {
                    "comment": {
                        "text": [
                            "```html",
                            "<igx-grid [igxGridState]=\"options\"></igx-grid>",
                            "```",
                            "```typescript",
                            "public options = {selection: false, advancedFiltering: false};",

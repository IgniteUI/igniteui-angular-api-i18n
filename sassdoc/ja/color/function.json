{
    "text-contrast": {
        "description": [
            "<p>渡された色のコントラスト色を返します。</p>",
            ""
        ],
        "parameters": {
            "background": {
                "description": [
                    "<p>コントラスト色/前景色を返すために使用される背景色。</p>",
                    ""
                ]
            },
            "foreground": {
                "description": [
                    "<p>指定された背景で使用できる",
                    "前景色のリスト。</p>",
                    ""
                ]
            },
            "contrast": {
                "description": [
                    "<p>WCAG 2.0 規格に準拠したコントラスト レベル。</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>必要なコントラスト レベルを満たす場合は、",
                "白、黒、または指定された前景色のいずれかを返します。</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    ".my-component {",
                    "   background: #09f",
                    "   color: text-contrast(#09f)",
                    " }"
                ]
            }
        ]
    },
    "to-hsl": {
        "description": [
            "<p>指定された色の色相、彩度、および明度の値のコンマ区切りリストを返します。</p>",
            ""
        ],
        "parameters": {
            "color": {
                "description": [
                    "<p>HSL 値リストに変換される色。</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>この HSL 値のリスト。</p>",
                ""
            ]
        },
        "example": [
            {
                "description": [
                    "<p>#000 を HSL 値リストに変換します。</p>",
                    ""
                ],
                "code": [
                    "$hsl-list: to-hsl(#000); // (0deg, 0%, 0%);"
                ]
            }
        ]
    },
    "contrast": {
        "description": [
            "<p>2 つの色の中間の色のコントラストの比率を計算します。",
            "<a href=\"https://www.w3.org/TR/WCAG20-TECHS/G17.html#G17-tests\">https://www.w3.org/TR/WCAG20-TECHS/G17.html#G17-tests</a> を参照してください</p>",
            ""
        ],
        "parameters": {
            "background": {
                "description": [
                    "<p>背景色。</p>",
                    ""
                ]
            },
            "foreground": {
                "description": [
                    "<p>前景色。</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>背景色と前景色のコントラストの比率。</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "contrast(#09f, #000);"
                ]
            }
        ]
    },
    "luminance": {
        "description": [
            "<p>指定された色の輝度を計算します。",
            "<a href=\"https://www.w3.org/TR/WCAG20-TECHS/G17.html#G17-tests\">https://www.w3.org/TR/WCAG20-TECHS/G17.html#G17-tests</a> を参照してください。</p>",
            ""
        ],
        "parameters": {
            "color": {
                "description": [
                    "<p>輝度が計算するための色。</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>特定の色の計算された輝度。</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "luminance(#09f);"
                ]
            }
        ]
    }
}
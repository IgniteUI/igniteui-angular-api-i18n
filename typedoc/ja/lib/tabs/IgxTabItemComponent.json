        "properties": {
            "relatedGroup": {
                "comment": {
                    "shortText": [
                        "タブに関連付けられているグループを取得します。",
                        "```html",
                        "const relatedGroup = this.tabbar.tabs.toArray()[1].relatedGroup;",
                        "```"
                    ]
                }
            }
        },
        "methods": {},
        "accessors": {
            "disabled": {
                "getter": {
                    "comment": {
                        "shortText": [
                            "\tタブが無効かどうかを取得します。",
                            "```",
                            "const disabledItem = this.myTabComponent.tabs.first.disabled;",
                            "```"
                        ]
                    }
                }
            },
            "isSelected": {
                "getter": {
                    "comment": {
                        "shortText": [
                            "タブが選択されているかどうかを取得します。",
                            "```typescript",
                            "const selectedItem = this.myTabComponent.tabs.first.isSelected;",
                            "```"
                        ]
                    }
                }
            }
        }
    }
}

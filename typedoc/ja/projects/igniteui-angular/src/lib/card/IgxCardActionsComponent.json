{
    "IgxCardActionsComponent": {
        "comment": {
            "blockTags": [],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "IgxCardActions はカード操作のためのコンテナーです。"
                }
            ]
        },
        "properties": {
            "layout": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@Input",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "アクションのレイアウト スタイルを設定する @Input プロパティ。\nigx-card-action コンテナーに挿入された要素を\n揃えて、カード アクション軸に沿って使用可能なすべてのスペースを\n占有して互いに均等に配置できます。"
                                }
                            ]
                        },
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-card-actions layout=\"justify\"></igx-card-actions>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "アクションのレイアウト スタイルを設定します。"
                        }
                    ]
                }
            },
            "vertical": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@Input",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "プロパティ。\n`true` "
                                },
                                {
                                    "kind": "code",
                                    "text": "に設定すると、"
                                },
                                {
                                    "kind": "text",
                                    "text": "アクションは縦にレイアウトされます。"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "アクションの垂直方向の属性を設定します。"
                        }
                    ]
                }
            },
            "reverse": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@非推奨",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "バージョン 15.1.0。\n\n"
                                }
                            ]
                        },
                        {
                            "tag": "@Input",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "アクション領域のボタンの順序を逆にする @Input プロパティ。"
                                }
                            ]
                        },
                        {
                            "tag": "@例",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-card-actions [reverse]=\"true\"></igx-card-actions>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": []
                }
            }
        },
        "methods": {},
        "accessors": {
            "isJustifyLayout": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "レイアウトが "
                            },
                            {
                                "kind": "code",
                                "text": "`justify`"
                            },
                            {
                                "kind": "text",
                                "text": " に設定されている\nときに "
                            },
                            {
                                "kind": "code",
                                "text": "`true`"
                            },
                            {
                                "kind": "text",
                                "text": "を返すゲッター。"
                            }
                        ]
                    }
                }
            }
        }
    }
}
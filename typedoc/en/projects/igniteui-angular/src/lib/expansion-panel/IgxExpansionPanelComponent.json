{
    "IgxExpansionPanelComponent": {
        "properties": {
            "id": {
                "comment": {
                    "tags": {
                        "memberof": {
                            "comment": {
                                "text": "IgxExpansionPanelComponent",
                                "tagName": "memberof"
                            }
                        }
                    },
                    "shortText": [
                        "Sets/gets the `id` of the expansion panel component.",
                        "If not set, `id` will have value `\"igx-expansion-panel-0\"`;",
                        "```html",
                        "<igx-expansion-panel id = \"my-first-expansion-panel\"></igx-expansion-panel>",
                        "```",
                        "```typescript",
                        "let panelId =  this.panel.id;",
                        "```"
                    ]
                }
            },
            "onCollapsed": {
                "comment": {
                    "shortText": [
                        "Emitted when the expansion panel finishes collapsing",
                        "```typescript",
                        " handleCollapsed(event: IExpansionPanelEventArgs)",
                        "```",
                        "```html",
                        " <igx-expansion-panel (onCollapsed)=\"handleCollapsed($event)\">",
                        "     ...",
                        " </igx-expansion-panel>",
                        "```"
                    ]
                }
            },
            "onExpanded": {
                "comment": {
                    "shortText": [
                        "Emitted when the expansion panel finishes expanding",
                        "```typescript",
                        " handleExpanded(event: IExpansionPanelEventArgs)",
                        "```",
                        "```html",
                        " <igx-expansion-panel (onExpanded)=\"handleExpanded($event)\">",
                        "     ...",
                        " </igx-expansion-panel>",
                        "```"
                    ]
                }
            }
        },
        "methods": {
            "collapse": {
                "comment": {
                    "text": [
                        "```html",
                        " <igx-expansion-panel #myPanel>",
                        "     ...",
                        " </igx-expansion-panel>",
                        " <button (click)=\"myPanel.collapse($event)\">Collpase Panel</button>",
                        "```"
                    ],
                    "shortText": "Collapses the panel"
                }
            },
            "expand": {
                "comment": {
                    "text": [
                        "```html",
                        " <igx-expansion-panel #myPanel>",
                        "     ...",
                        " </igx-expansion-panel>",
                        " <button (click)=\"myPanel.expand($event)\">Expand Panel</button>",
                        "```"
                    ],
                    "shortText": "Expands the panel"
                }
            },
            "toggle": {
                "comment": {
                    "text": [
                        "```html",
                        " <igx-expansion-panel #myPanel>",
                        "     ...",
                        " </igx-expansion-panel>",
                        " <button (click)=\"myPanel.toggle($event)\">Expand Panel</button>",
                        "```"
                    ],
                    "shortText": "Toggles the panel"
                }
            }
        },
        "accessors": {
            "animationSettings": {
                "getter": {
                    "comment": {
                        "text": [
                            "Get",
                            "```typescript",
                            " const currentAnimations = this.panel.animationSettings;",
                            "```",
                            "Set",
                            "```typescript",
                            " import { slideInLeft, slideOutRight } from 'igniteui-angular';",
                            " ...",
                            " this.panel.animationsSettings = {",
                            "     openAnimation: slideInLeft,",
                            "     closeAnimation: slideOutRight",
                            "};",
                            "```",
                            "or via template",
                            "```typescript",
                            " import { slideInLeft, slideOutRight } from 'igniteui-angular';",
                            " ...",
                            " myCustomAnimationObject = {",
                            "     openAnimation: slideInLeft,",
                            "     closeAnimation: slideOutRight",
                            "};",
                            "```html",
                            " <igx-expansion-panel [animationSettings]='myCustomAnimationObject'>",
                            " ...",
                            " </igx-expansion-panel>",
                            "```"
                        ],
                        "shortText": [
                            "Sets/gets the animation settings of the expansion panel component",
                            "Open and Close animation should be passed"
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "text": [
                            "Get",
                            "```typescript",
                            " const currentAnimations = this.panel.animationSettings;",
                            "```",
                            "Set",
                            "```typescript",
                            " import { slideInLeft, slideOutRight } from 'igniteui-angular';",
                            " ...",
                            " this.panel.animationsSettings = {",
                            "     openAnimation: slideInLeft,",
                            "     closeAnimation: slideOutRight",
                            "};",
                            "```",
                            "or via template",
                            "```typescript",
                            " import { slideInLeft, slideOutRight } from 'igniteui-angular';",
                            " ...",
                            " myCustomAnimationObject = {",
                            "     openAnimation: slideInLeft,",
                            "     closeAnimation: slideOutRight",
                            "};",
                            "```html",
                            " <igx-expansion-panel [animationSettings]='myCustomAnimationObject'>",
                            " ...",
                            " </igx-expansion-panel>",
                            "```"
                        ],
                        "shortText": [
                            "Sets/gets the animation settings of the expansion panel component",
                            "Open and Close animation should be passed"
                        ]
                    }
                }
            },
            "collapsed": {
                "getter": {
                    "comment": {
                        "text": [
                            "Two-way data binding:",
                            "```html",
                            "<igx-expansion-panel [(collapsed)]=\"model.isCollapsed\"></igx-expansion-panel>",
                            "```"
                        ],
                        "shortText": [
                            "Gets/sets whether the component is collapsed (its content is hidden)",
                            "Get",
                            "```typescript",
                            " const myPanelState: boolean = this.panel.collapsed;",
                            "```",
                            "Set",
                            "```html",
                            " this.panel.collapsed = true;",
                            "```"
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "text": [
                            "Two-way data binding:",
                            "```html",
                            "<igx-expansion-panel [(collapsed)]=\"model.isCollapsed\"></igx-expansion-panel>",
                            "```"
                        ],
                        "shortText": [
                            "Gets/sets whether the component is collapsed (its content is hidden)",
                            "Get",
                            "```typescript",
                            " const myPanelState: boolean = this.panel.collapsed;",
                            "```",
                            "Set",
                            "```html",
                            " this.panel.collapsed = true;",
                            "```"
                        ]
                    }
                }
            }
        }
    }
}
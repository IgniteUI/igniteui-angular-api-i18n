                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "このイベントは、エクスポート処理が終了したときに発生します。\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\nthis.exporterService.exportEnded.subscribe((args: IExcelExportEndedEventArgs) => {\n// put event handler code here\n});\n```"
                        }
                    ]
                }
            },

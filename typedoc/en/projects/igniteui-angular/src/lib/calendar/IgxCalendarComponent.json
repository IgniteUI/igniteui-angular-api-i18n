                    }
                },
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the current active view.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```html\n<igx-calendar [activeView]=\"year\" #calendar></igx-calendar>\n```"
                            },
                            {
                                "kind": "text",
                                "text": "\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\ncalendar.activeView = IgxCalendarView.YEAR;\n```"
                            }
                        ]
                    }
                }
            },
            "resourceStrings": {
                "getter": {
                    "comment": {
                        "blockTags": [],

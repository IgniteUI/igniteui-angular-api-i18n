{
    "IgxExpansionPanelComponent": {
        "properties": {
            "id": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@memberof",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "IgxExpansionPanelComponent"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets/gets the "
                        },
                        {
                            "kind": "code",
                            "text": "`id`"
                        },
                        {
                            "kind": "text",
                            "text": " of the expansion panel component.\nIf not set, "
                        },
                        {
                            "kind": "code",
                            "text": "`id`"
                        },
                        {
                            "kind": "text",
                            "text": " will have value "
                        },
                        {
                            "kind": "code",
                            "text": "`\"igx-expansion-panel-0\"`"
                        },
                        {
                            "kind": "text",
                            "text": ";\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-expansion-panel id = \"my-first-expansion-panel\"></igx-expansion-panel>\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\nlet panelId =  this.panel.id;\n```"
                        }
                    ]
                }
            },
            "collapsed": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Gets/sets whether the component is collapsed (its content is hidden)\nGet\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n const myPanelState: boolean = this.panel.collapsed;\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\nSet\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n this.panel.collapsed = true;\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n\nTwo-way data binding:\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-expansion-panel [(collapsed)]=\"model.isCollapsed\"></igx-expansion-panel>\n```"
                        }
                    ]
                }
            },
            "contentCollapsing": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emitted when the expansion panel starts collapsing\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n handleCollapsing(event: IExpansionPanelCancelableEventArgs)\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n <igx-expansion-panel (contentCollapsing)=\"handleCollapsing($event)\">\n     ...\n </igx-expansion-panel>\n```"
                        }
                    ]
                }
            },
            "contentCollapsed": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emitted when the expansion panel finishes collapsing\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n handleCollapsed(event: IExpansionPanelEventArgs)\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n <igx-expansion-panel (contentCollapsed)=\"handleCollapsed($event)\">\n     ...\n </igx-expansion-panel>\n```"
                        }
                    ]
                }
            },
            "contentExpanding": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emitted when the expansion panel starts expanding\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n handleExpanding(event: IExpansionPanelCancelableEventArgs)\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n <igx-expansion-panel (contentExpanding)=\"handleExpanding($event)\">\n     ...\n </igx-expansion-panel>\n```"
                        }
                    ]
                }
            },
            "contentExpanded": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emitted when the expansion panel finishes expanding\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\n handleExpanded(event: IExpansionPanelEventArgs)\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n <igx-expansion-panel (contentExpanded)=\"handleExpanded($event)\">\n     ...\n </igx-expansion-panel>\n```"
                        }
                    ]
                }
            }
        },
        "methods": {
            "collapse": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Collapses the panel\n\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n <igx-expansion-panel #myPanel>\n     ...\n </igx-expansion-panel>\n <button (click)=\"myPanel.collapse($event)\">Collpase Panel</button>\n```"
                        }
                    ]
                }
            },
            "expand": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Expands the panel\n\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n <igx-expansion-panel #myPanel>\n     ...\n </igx-expansion-panel>\n <button (click)=\"myPanel.expand($event)\">Expand Panel</button>\n```"
                        }
                    ]
                }
            },
            "toggle": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Toggles the panel\n\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n <igx-expansion-panel #myPanel>\n     ...\n </igx-expansion-panel>\n <button (click)=\"myPanel.toggle($event)\">Expand Panel</button>\n```"
                        }
                    ]
                }
            }
        },
        "accessors": {
            "animationSettings": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets/gets the animation settings of the expansion panel component\nOpen and Close animation should be passed\n\nGet\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\n const currentAnimations = this.panel.animationSettings;\n```"
                            },
                            {
                                "kind": "text",
                                "text": "\nSet\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\n import { slideInLeft, slideOutRight } from 'igniteui-angular';\n ...\n this.panel.animationsSettings = {\n     openAnimation: slideInLeft,\n     closeAnimation: slideOutRight\n};\n```"
                            },
                            {
                                "kind": "text",
                                "text": "\nor via template\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\n import { slideInLeft, slideOutRight } from 'igniteui-angular';\n ...\n myCustomAnimationObject = {\n     openAnimation: slideInLeft,\n     closeAnimation: slideOutRight\n};\n```"
                            },
                            {
                                "kind": "text",
                                "text": "html\n <igx-expansion-panel [animationSettings]='myCustomAnimationObject'>\n ...\n </igx-expansion-panel>\n```"
                            }
                        ]
                    }
                }
            }
        }
    }
}
{
    "palette": {
        "description": [
            "<p>カラー パレットを生成します。</p>",
            ""
        ],
        "parameters": {
            "primary": {
                "description": [
                    "<p>基本パレットの生成に使用する primary カラー (必須)。</p>",
                    ""
                ]
            },
            "secondary": {
                "description": [
                    "<p>二次色パレットの生成に使用する secondary カラー (必須)。</p>",
                    ""
                ]
            },
            "surface": {
                "description": [
                    "<p>コンポーネントに使用されるカラー (必須)。</p>",
                    ""
                ]
            },
            "gray": {
                "description": [
                    "<p>グレースケール パレットの生成に使用するカラー (オプション)。</p>",
                    ""
                ]
            },
            "info": {
                "description": [
                    "<p>アプリケーション内で使用する information カラー (オプション)。</p>",
                    ""
                ]
            },
            "success": {
                "description": [
                    "<p>アプリケーション内で使用する success カラー (オプション)。</p>",
                    ""
                ]
            },
            "warn": {
                "description": [
                    "<p>アプリケーション内で使用する warning カラー (オプション)。</p>",
                    ""
                ]
            },
            "error": {
                "description": [
                    "<p>アプリケーション内で使用する error カラー (オプション)。</p>",
                    ""
                ]
            },
            "variant": {
                "description": [
                    "<p>内部で使用されます (オプション)。</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>渡された基本色 (primary、secondary、gray など) の色合いで構成されるマップ。</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "// $primary、$secondary、$surface は必須パラメーターです。",
                    "$my-palette: palette($primary: #09f,$secondary #222, $surface: #fff);"
                ]
            }
        ]
    },
    "shades": {
        "description": [
            "<p>基本色からマテリアルのようなカラー パレットを生成します。</p>",
            ""
        ],
        "parameters": {
            "name": {
                "description": [
                    "<p>カラーの名前。</p>",
                    ""
                ]
            },
            "color": {
                "description": [
                    "<p>パレットの生成に使用する基本色。</p>",
                    ""
                ]
            },
            "shades": {
                "description": [
                    "<p>色合いのリスト。</p>",
                    ""
                ]
            },
            "surface": {
                "description": [
                    "<p>surface のカラー。gray の色合いを生成するときに便利です (オプション)。</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>hsla 色合いとそれぞれのコントラスト カラーで構成されるマップ。</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "$special-color-shades: shades('accent', #09f, (50,100,200,300,400,500,600,700,800,900))"
                ]
            }
        ]
    },
    "color": {
        "description": [
            "<p>カラーパレットから色を取得します。</p>",
            ""
        ],
        "parameters": {
            "palette": {
                "description": [
                    "<p>ソースのパレット マップ。(オプション)</p>",
                    ""
                ]
            },
            "color": {
                "description": [
                    "<p>カラー パレットからターゲット色を取得します。</p>",
                    ""
                ]
            },
            "variant": {
                "description": [
                    "<p>カラー パレットからのターゲット色の影</p>",
                    ""
                ]
            },
            "opacity": {
                "description": [
                    "<p>色合いに適用するオプションの不透明度。</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>パレットからの生の色合いまたは CSS 変数参照。</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "// No palette",
                    ".my-component-1 {",
                    "   background: color($color: 'primary', $variant: 200);",
                    " }",
                    " // Using certain palette and adding .5% opacity to the color variant",
                    ".my-component-2 {",
                    "   background: color($my-palette, 'primary', 200, .5);",
                    " }"
                ]
            }
        ]
    },
    "contrast-color": {
        "description": [
            "<p>カラー レットから指定のカラー バリアントのためコントラスト テキスト色を取得します。</p>",
            ""
        ],
        "parameters": {
            "palette": {
                "description": [
                    "<p>ソースのパレット マップ。(オプション)</p>",
                    ""
                ]
            },
            "color": {
                "description": [
                    "<p>カラー パレットからターゲット色を取得します。</p>",
                    ""
                ]
            },
            "variant": {
                "description": [
                    "<p>カラー パレットからのターゲット色の影。</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>パレットからの生のコントラスト色合いまたは CSS 変数。</p>",
                ""
            ]
        },
        "example": [
            {
                "description": [
                    "<p>パレットを通さずに</p>",
                    ""
                ],
                "code": [
                    ".my-component {",
                    "   background: color($color: 'primary', $variant: 200);",
                    "   color: contrast-color($color: 'primary', $variant: 200);",
                    " }"
                ]
            }
        ]
    },
    "chart-brushes": {
        "description": [
            "<p>チャート ブラシとして使用される色のリストを返します。</p>",
            ""
        ],
        "returns": {
            "description": [
                "<p>チャート ブラシとして使用される カラーのリスト。</p>",
                ""
            ]
        }
    }
}
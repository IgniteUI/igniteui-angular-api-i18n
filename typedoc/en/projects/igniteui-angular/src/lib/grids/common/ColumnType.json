            "isFirstPinned": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Indicates whether the current column is the first for the grid to be pinned.\nIf the value is false, there are columns, that have been pinned before the current"

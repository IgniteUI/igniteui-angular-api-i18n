{
    "IgxMonthPickerBaseDirective": {
        "properties": {
            "hideOutsideDays": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "現在の月以外の日付を非表示にするかどうかを設定または取得します。\nデフォルト値は "

    "type-style": {
        "description": [
            "<p>Produces a type style map of values that adheres to the ITypeStyle map.</p>",
            ""
        ],
        "parameters": {
            "font-family": {
                "description": [
                    "<p>The font family of the type style.</p>",
                    ""
                ]
            },
            "font-size": {
                "description": [
                    "<p>The font size of the type style.</p>",
                    ""
                ]
            },
            "font-weight": {
                "description": [
                    "<p>The font weight of the type style.</p>",
                    ""
                ]
            },
            "font-style": {
                "description": [
                    "<p>The font style of the type style.</p>",
                    ""
                ]
            },
            "line-height": {
                "description": [
                    "<p>The line height of the type style.</p>",
                    ""
                ]
            },
            "letter-spacing": {
                "description": [
                    "<p>The letter spacing of the type style.</p>",
                    ""
                ]
            },
            "text-transform": {
                "description": [
                    "<p>The text-transform property of the type style.</p>",
                    ""
                ]
            },
            "margin-top": {
                "description": [
                    "<p>The margin-top property of the type style.</p>",
                    ""
                ]
            },
            "margin-bottom": {
                "description": [
                    "<p>The margin-bottom property of the type style.</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>A map of all defined type style properties.</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "$main_navigation: type-style(",
                    "   $font-size: rem(14px),",
                    "   $font-weight: 600,",
                    "   $letter-spacing: rem(.1px),",
                    "   $line-height: rem(14px),",
                    "   $text-transform: none,",
                    ");",
                    "",
                    "// Use the type-style() mixin to consume the new type style produced from the type-style() function.",
                    ".main-nav {",
                    "  @include type-style($main_navigation);",
                    "}"
                ]
            }
        ]
    },

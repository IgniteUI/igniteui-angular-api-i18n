{
    "IgxDividerDirective": {
        "properties": {
            "id": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets/gets the "
                        },
                        {
                            "kind": "code",
                            "text": "`id`"
                        },
                        {
                            "kind": "text",
                            "text": " of the divider.\nIf not set, "
                        },
                        {
                            "kind": "code",
                            "text": "`id`"
                        },
                        {
                            "kind": "text",
                            "text": " will have value "
                        },
                        {
                            "kind": "code",
                            "text": "`\"igx-divider-0\"`"
                        },
                        {
                            "kind": "text",
                            "text": ";\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-divider id=\"my-divider\"></igx-divider>\n```"
                        },
                        {
                            "kind": "text",
                            "text": "\n"
                        },
                        {
                            "kind": "code",
                            "text": "```typescript\nlet dividerId =  this.divider.id;\n```"
                        }
                    ]
                }
            },
            "role": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@Input",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "property that sets the value of "
                                },
                                {
                                    "kind": "code",
                                    "text": "`role`"
                                },
                                {
                                    "kind": "text",
                                    "text": " attribute.\nIf not the default value of "
                                },
                                {
                                    "kind": "code",
                                    "text": "`separator`"
                                },
                                {
                                    "kind": "text",
                                    "text": " will be used."
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "An"
                        }
                    ]
                }
            },
            "type": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets the type of the divider. The default value\nis "
                        },
                        {
                            "kind": "code",
                            "text": "`default`"
                        },
                        {
                            "kind": "text",
                            "text": ". The divider can also be "
                        },
                        {
                            "kind": "code",
                            "text": "`dashed`"
                        },
                        {
                            "kind": "text",
                            "text": ";\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<igx-divider type=\"dashed\"></igx-divider>\n```"
                        }
                    ]
                }
            },
            "middle": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@Input",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "that sets the "
                                },
                                {
                                    "kind": "code",
                                    "text": "`middle`"
                                },
                                {
                                    "kind": "text",
                                    "text": " attribute of the divider.\nIf set to "
                                },
                                {
                                    "kind": "code",
                                    "text": "`true`"
                                },
                                {
                                    "kind": "text",
                                    "text": " and an "
                                },
                                {
                                    "kind": "code",
                                    "text": "`inset`"
                                },
                                {
                                    "kind": "text",
                                    "text": " value has been provided,\nthe divider will start shrinking from both ends.\n"
                                },
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-divider [middle]=\"true\"></igx-divider>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "An"
                        }
                    ]
                }
            },
            "vertical": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@Input",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "that sets the vertical attribute of the divider.\n"
                                },
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-divider [vertical]=\"true\"></igx-divider>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "An"
                        }
                    ]
                }
            }
        },
        "methods": {},
        "accessors": {
            "inset": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [

                                        "public icon: IgxIconComponent;",
                                        "ngAfterViewInit() {",
                                        "   let iconActive = this.icon.getInactive;",
                                        "}",
                                        "```"
                                    ],
                                    "tagName": "例:"
                                }
                            }
                        },
                        "shortText": "inactive プロパティを返すアクセサー。"
                    }
                }
            },
            "getColor": {
                "getter": {
                    "comment": {

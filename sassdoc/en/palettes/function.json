{
    "palette": {
        "description": [
            "<p>Generates a color palette.</p>",
            ""
        ],
        "parameters": {
            "primary": {
                "description": [
                    "<p>The primary color used to generate the primary color palette (required).</p>",
                    ""
                ]
            },
            "secondary": {
                "description": [
                    "<p>The secondary color used to generate the secondary color palette (required).</p>",
                    ""
                ]
            },
            "surface": {
                "description": [
                    "<p>The color used as a background in components, such as cards, sheets, and menus (required).</p>",
                    ""
                ]
            },
            "gray": {
                "description": [
                    "<p>The color used for generating the grayscale palette (optional).</p>",
                    ""
                ]
            },
            "info": {
                "description": [
                    "<p>The information color used throughout the application (optional).</p>",
                    ""
                ]
            },
            "success": {
                "description": [
                    "<p>The success color used throughout the application (optional).</p>",
                    ""
                ]
            },
            "warn": {
                "description": [
                    "<p>The warning color used throughout the application (optional).</p>",
                    ""
                ]
            },
            "error": {
                "description": [
                    "<p>The error color used throughout the application (optional).</p>",
                    ""
                ]
            },
            "variant": {
                "description": [
                    "<p>Used internally (optional).</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>A map consisting of color shades for the passed base colors (primary, secondary, gray, etc).</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "// $primary, $secondary, $surface are required parameters.",
                    "$my-palette: palette($primary: #09f,$secondary #222, $surface: #fff);"
                ]
            }
        ]
    },
    "shades": {
        "description": [
            "<p>Generates a Material-like color palette from a base color.</p>",
            ""
        ],
        "parameters": {
            "name": {
                "description": [
                    "<p>The name of the color.</p>",
                    ""
                ]
            },
            "color": {
                "description": [
                    "<p>The base color used to generate the palette.</p>",
                    ""
                ]
            },
            "shades": {
                "description": [
                    "<p>The list of shades.</p>",
                    ""
                ]
            },
            "surface": {
                "description": [
                    "<p>The surface color. Useful when generating shades of gray (optional).</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>A map consisting of hsla color shades and their respective contrast colors.</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "$special-color-shades: shades('accent', #09f, (50,100,200,300,400,500,600,700,800,900))"
                ]
            }
        ]
    },
    "color": {
        "description": [
            "<p>Retrieves a color from a color palette.</p>",
            ""
        ],
        "parameters": {
            "palette": {
                "description": [
                    "<p>The source palette map (optional).</p>",
                    ""
                ]
            },
            "color": {
                "description": [
                    "<p>The target color from the color palette.</p>",
                    ""
                ]
            },
            "variant": {
                "description": [
                    "<p>The target color shade from the color palette.</p>",
                    ""
                ]
            },
            "opacity": {
                "description": [
                    "<p>Optional opacity to apply to the color shade.</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>The raw color shade or CSS variable reference from the palette.</p>",
                ""
            ]
        },
        "example": [
            {
                "code": [
                    "// No palette",
                    ".my-component-1 {",
                    "   background: color($color: 'primary', $variant: 200);",
                    " }",
                    " // Using certain palette and adding .5% opacity to the color variant",
                    ".my-component-2 {",
                    "   background: color($my-palette, 'primary', 200, .5);",
                    " }"
                ]
            }
        ]
    },
    "contrast-color": {
        "description": [
            "<p>Retrieves a contrast text color for a given color variant from a color palette.</p>",
            ""
        ],
        "parameters": {
            "palette": {
                "description": [
                    "<p>The source palette map (optional).</p>",
                    ""
                ]
            },
            "color": {
                "description": [
                    "<p>The target color from the color palette.</p>",
                    ""
                ]
            },
            "variant": {
                "description": [
                    "<p>The target color shade from the color palette.</p>",
                    ""
                ]
            }
        },
        "returns": {
            "description": [
                "<p>The raw contrast color shade or CSS variable from the palette.</p>",
                ""
            ]
        },
        "example": [
            {
                "description": [
                    "<p>without passing a palette</p>",
                    ""
                ],
                "code": [
                    ".my-component {",
                    "   background: color($color: 'primary', $variant: 200);",
                    "   color: contrast-color($color: 'primary', $variant: 200);",
                    " }"
                ]
            }
        ]
    },
    "chart-brushes": {
        "description": [
            "<p>Returns a list of colors to be used as chart brushes.</p>",
            ""
        ],
        "returns": {
            "description": [
                "<p>A list of colors to be used as chart brushes.</p>",
                ""
            ]
        }
    }
}
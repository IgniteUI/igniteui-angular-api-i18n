{
    "IgxAvatarComponent": {
        "comment": {
            "blockTags": [
                {
                    "tag": "@igxModule",
                    "content": [
                        {
                            "kind": "text",
                            "text": "IgxAvatarModule"
                        }
                    ]
                },
                {
                    "tag": "@igxTheme",
                    "content": [
                        {
                            "kind": "text",
                            "text": "igx-avatar-theme, igx-icon-theme"
                        }
                    ]
                },
                {
                    "tag": "@igxKeywords",
                    "content": [
                        {
                            "kind": "text",
                            "text": "avatar, profile, picture, initials"
                        }
                    ]
                },
                {
                    "tag": "@igxGroup",
                    "content": [
                        {
                            "kind": "text",
                            "text": "Layouts"
                        }
                    ]
                },
                {
                    "tag": "@remarks",
                    "content": [
                        {
                            "kind": "text",
                            "text": "The Ignite UI Avatar provides an easy way to add an avatar icon to your application.  This icon can be an\nimage, someone's initials or a material icon from the Google Material icon set."
                        }
                    ]
                },
                {
                    "tag": "@example",
                    "content": [
                        {
                            "kind": "code",
                            "text": "```html\n<igx-avatar initials=\"MS\" shape=\"rounded\" size=\"large\">\n</igx-avatar>\n```"
                        }
                    ]
                }
            ],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "Avatar provides a way to display an image, icon or initials to the user."
                }
            ]
        },
        "properties": {
            "ariaLabel": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```typescript\nlet ariaLabel = this.avatar.ariaLabel;\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Returns the "
                        },
                        {
                            "kind": "code",
                            "text": "`aria-label`"
                        },
                        {
                            "kind": "text",
                            "text": " attribute of the avatar."
                        }
                    ]
                }
            },
            "role": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```typescript\nlet avatarRole = this.avatar.role;\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Returns the "
                        },
                        {
                            "kind": "code",
                            "text": "`role`"
                        },
                        {
                            "kind": "text",
                            "text": " attribute of the avatar."
                        }
                    ]
                }
            },
            "roleDescription": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```typescript\nlet avatarDescription = this.avatar.roleDescription;\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Returns the type of the avatar.\nThe avatar can be:\n- "
                        },
                        {
                            "kind": "code",
                            "text": "`\"initials type avatar\"`"
                        },
                        {
                            "kind": "text",
                            "text": "\n- "
                        },
                        {
                            "kind": "code",
                            "text": "`\"icon type avatar\"`"
                        },
                        {
                            "kind": "text",
                            "text": "\n- "
                        },
                        {
                            "kind": "code",
                            "text": "`\"image type avatar\"`"
                        },
                        {
                            "kind": "text",
                            "text": ".\n- "
                        },
                        {
                            "kind": "code",
                            "text": "`\"custom type avatar\"`"
                        },
                        {
                            "kind": "text",
                            "text": "."
                        }
                    ]
                }
            },
            "id": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-avatar id=\"my-first-avatar\"></igx-avatar>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets the "
                        },
                        {
                            "kind": "code",
                            "text": "`id`"
                        },
                        {
                            "kind": "text",
                            "text": " of the avatar. If not set, the first avatar component will have "
                        },
                        {
                            "kind": "code",
                            "text": "`id`"
                        },
                        {
                            "kind": "text",
                            "text": " = "
                        },
                        {
                            "kind": "code",
                            "text": "`\"igx-avatar-0\"`"
                        },
                        {
                            "kind": "text",
                            "text": "."
                        }
                    ]
                }
            },
            "shape": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-avatar shape=\"rounded\"></igx-avatar>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets square, rounded or circular shape to the avatar.\nBy default the shape of the avatar is square."
                        }
                    ]
                }
            },
            "color": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-avatar color=\"blue\"></igx-avatar>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets the color of the avatar's initials or icon."
                        }
                    ]
                }
            },
            "bgColor": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-avatar bgColor=\"yellow\"></igx-avatar>\n```"
                                }
                            ]
                        },
                        {
                            "tag": "@igxFriendlyName",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "Background color"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets the background color of the avatar."
                        }
                    ]
                }
            },
            "initials": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-avatar initials=\"MN\"></igx-avatar>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets initials to the avatar."
                        }
                    ]
                }
            },
            "icon": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-avatar icon=\"phone\"></igx-avatar>\n```"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets an icon to the avatar. All icons from the material icon set are supported."
                        }
                    ]
                }
            },
            "src": {
                "comment": {
                    "blockTags": [
                        {
                            "tag": "@example",
                            "content": [
                                {
                                    "kind": "code",
                                    "text": "```html\n<igx-avatar src=\"images/picture.jpg\"></igx-avatar>\n```"
                                }
                            ]
                        },
                        {
                            "tag": "@igxFriendlyName",
                            "content": [
                                {
                                    "kind": "text",
                                    "text": "Image URL"
                                }
                            ]
                        }
                    ],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets the image source of the avatar."
                        }
                    ]
                }
            }
        },
        "methods": {},
        "accessors": {
            "size": {
                "getter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```typescript\nlet avatarSize = this.avatar.size;\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Returns the size of the avatar."
                            }
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```html\n<igx-avatar size=\"large\"></igx-avatar>\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the size  of the avatar.\nBy default, the size is "
                            },
                            {
                                "kind": "code",
                                "text": "`\"small\"`"
                            },
                            {
                                "kind": "text",
                                "text": ". It can be set to "
                            },
                            {
                                "kind": "code",
                                "text": "`\"medium\"`"
                            },
                            {
                                "kind": "text",
                                "text": " or "
                            },
                            {
                                "kind": "code",
                                "text": "`\"large\"`"
                            },
                            {
                                "kind": "text",
                                "text": "."
                            }
                        ]
                    }
                }
            },
            "type": {
                "getter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@example",
                                "content": [
                                    {
                                        "kind": "code",
                                        "text": "```typescript\nlet avatarType = this.avatar.type;\n```"
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Returns the type of the avatar."
                            }
                        ]
                    }
                }
            }
        }
    }
}
                    }
                },
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "行がピン固定されるかどうかを設定します。\nデフォルト値は "
                            },
                            {
                                "kind": "code",
                                "text": "`false`"
                            },
                            {
                                "kind": "text",
                                "text": " です。\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\nthis.grid.selectedRows[0].pinned = true;\n```"
                            }
                        ]
                    }
                }
            },
            "expanded": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [

                "code": [
                    "%igx-avatar-icon {",
                    "  color: --var($avatar-theme, 'icon-color');",
                    "}",
                    "// legacy-support がオフの場合、次のを生成します。",
                    "// %igx-avatar-icon {",
                    "//   color: var(--igx-avatar-icon-color);",
                    "// }",
                    "// そうでない場合、'$avatar-theme’の 'icon-color’ キーの値を返します。;"

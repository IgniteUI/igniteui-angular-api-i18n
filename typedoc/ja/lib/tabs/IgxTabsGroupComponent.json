                "getter": {
                    "comment": {
                        "shortText": [
                            "`IgxTabItemComponent` コンポーネントを返すアクセサー。",
                            "```typescript",
                            "@ViewChild(\"MyTabsGroup\")",
                            "public tab: IgxTabsGroupComponent;",
                            "ngAfterViewInIt(){",
                            "   let tabComponent = this.tab.relatedTab;",
                            "}",
                            "```"
                        ]
                    }
                }
            },
            "index": {
                "getter": {
                    "comment": {
                        "shortText": [

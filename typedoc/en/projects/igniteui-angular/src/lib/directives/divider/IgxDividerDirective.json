                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the inset of the divider from the side(s).\nIf the divider attribute "
                            },
                            {
                                "kind": "code",
                                "text": "`middle`"
                            },
                            {
                                "kind": "text",
                                "text": " is set to "
                            },
                            {
                                "kind": "code",
                                "text": "`true`"
                            },
                            {
                                "kind": "text",
                                "text": ",\nit will inset the divider on both sides.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\nthis.divider.inset = '32px';\n```"
                            }
                        ]
                    }
                }
            },
            "isSolid": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "A getter that returns "
                            },
                            {
                                "kind": "code",
                                "text": "`true`"
                            },
                            {
                                "kind": "text",
                                "text": " if the type of the divider is "
                            },
                            {
                                "kind": "code",
                                "text": "`default`"
                            },
                            {
                                "kind": "text",
                                "text": ";\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\nconst isDefault = this.divider.isDefault;\n```"
                            }
                        ]
                    }
                }
            }
        }
    }
}
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the "
                            },
                            {
                                "kind": "code",
                                "text": "`locale`"
                            },
                            {
                                "kind": "text",
                                "text": " of the calendar.\nExpects a valid BCP 47 language tag."
                            }
                        ]
                    }
                }
            },
            "formatOptions": {

{
    "color-classes": {
        "description": [
            "<p>指定したプロパティとカラー パレットのすべての色",
            "の CSS クラス名を生成します。",
            "オプションでプレフィックスとサフィックスをクラス名に添付します。</p>",
            ""
        ],
        "parameters": {
            "prop": {
                "description": [
                    "<p>パレット カラーに割り当てられる CSS プロパティ。</p>",
                    ""
                ]
            },
            "prefix": {
                "description": [
                    "<p>クラス名に連結するプレフィックス。</p>",
                    ""
                ]
            },
            "suffix": {
                "description": [
                    "<p>クラス名に連結されるサフィックス。</p>",
                    ""
                ]
            },
            "palette": {
                "description": [
                    "<p>CSS クラス名の生成に使用する igx パレット。</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p>パレットの色でバックグラウンド クラスを生成します。</p>",
                    ""
                ],
                "code": [
                    "// 次のようなクラス名を生成します。",
                    "// .igx-primary-500-bg { ... };",
                    "@include color-classes(",
                    "  $prop: 'background-color',",
                    "  $suffix: 'bg'",
                    ");"
                ]
            }
        ]
    },
    "border-radius": {
        "example": [
            {
                "code": [
                    "@include border-radius(4px, 0px, 12px);"
                ]
            }
        ]
    },
    "line-clamp": {
        "description": [
            "<p>特定の行数でテキストを切り捨てます。</p>",
            ""
        ],
        "parameters": {
            "lines": {
                "description": [
                    "<p>表示する行数</p>",
                    ""
                ]
            },
            "inline": {
                "description": [
                    "<p>要素が inline-box または box を表示するかどうかを設定します。</p>",
                    ""
                ]
            },
            "vertical": {
                "description": [
                    "<p>要素がコンテンツを水平または垂直に配置するかどうかを設定します。</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p>4 行目以降のテキストを切り捨てます。</p>",
                    ""
                ],
                "code": [
                    "@include line-clamp(4, true, true);"
                ]
            }
        ]
    },
    "ellipsis": {
        "description": [
            "<p>E テキスト要素に text-overflow 省略記号を適用します。",
            "Display flex 要素では動作しません。</p>",
            ""
        ],
        "example": [
            {
                "code": [
                    ".my-class {",
                    "  @include ellipsis();",
                    "}"
                ]
            }
        ]
    },
    "hide-default": {
        "description": [
            "<p>DOM から要素を非表示にします。</p>",
            ""
        ],
        "example": [
            {
                "description": [
                    "<p>例</p>",
                    ""
                ],
                "code": [
                    "input[type=\"checkbox\"] {",
                    "  @include hide-default();",
                    "}"
                ]
            }
        ]
    }
}
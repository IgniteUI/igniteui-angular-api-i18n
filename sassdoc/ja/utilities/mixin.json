{
    "hide-default": {
        "description": [
            "<p>DOM から要素を非表示にします。</p>",
            ""
        ],
        "example": [
            {
                "description": [
                    "<p>例</p>",
                    ""
                ],
                "code": [
                    "input[type=\"checkbox\"] {",
                    "  @include hide-default();",
                    "}"
                ]
            }
        ]
    },
    "linear-gradient": {
        "description": [
            "<p>線状グラデーションを生成します。</p>",
            ""
        ],
        "parameters": {
            "direction": {
                "description": [
                    "<p>グラデーションの方向</p>",
                    ""
                ]
            },
            "color-stops": {
                "description": [
                    "<p>グラデーションに使用される色の境界リスト。</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p>例</p>",
                    ""
                ],
                "code": [
                    ".bozo {",
                    "  background: @include linear-gradient(\"to right\", red, orange);",
                    "}"
                ]
            }
        ]
    },
    "ellipsis": {
        "description": [
            "<p>テキスト要素に text-overflow 省略記号を適用します。",
            "Display flex 要素では動作しません。</p>",
            ""
        ]
    },
    "igx-palette-vars": {
        "description": [
            "<p>指定されたパレットのため CSS 変数を生成します。</p>",
            ""
        ],
        "parameters": {
            "palette": {
                "description": [
                    "<p>CSS 変数の生成に使用する igx パレット。</p>",
                    ""
                ]
            },
            "contrast": {
                "description": [
                    "<p>コントラスト色を含めるかどうかを指定します。</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p><code>$default-palette</code> のため生成された css 変数。</p>",
                    ""
                ],
                "code": [
                    "@include igx-palette-vars($default-palette);"
                ]
            }
        ]
    },
    "igx-elevation-vars": {
        "description": [
            "<p>指定したエレベーション マップの CSS 変数を生成します。</p>",
            ""
        ],
        "parameters": {
            "elevations": {
                "description": [
                    "<p>CSS 変数を生成するために使用する igx エレベーション マップ。</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p>エレベーションの CSS 変数を生成します。</p>",
                    ""
                ],
                "code": [
                    "$elevations: igx-elevations(red, green, blue); // umbra, penumbra, ambient",
                    "@include igx-elevation-vars($elevations);"
                ]
            }
        ]
    },
    "igx-color-classes": {
        "description": [
            "<p>指定したプロパティとカラー パレットの",
            "すべての色の CSS クラス名を生成します。",
            "オプションでプレフィックスとサフィックスをクラス名に添付します。</p>",
            ""
        ],
        "parameters": {
            "prop": {
                "description": [
                    "<p>色のパレットに割り当てられる CSS プロパティ。</p>",
                    ""
                ]
            },
            "prefix": {
                "description": [
                    "<p>クラス名に連結するプレフィックス。</p>",
                    ""
                ]
            },
            "suffix": {
                "description": [
                    "<p>クラス名に連結される接尾辞。</p>",
                    ""
                ]
            },
            "palette": {
                "description": [
                    "<p>CSS クラス名の生成に使用する igx パレット。</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p>パレットの色でバックグラウンド クラスを生成します。</p>",
                    ""
                ],
                "code": [
                    "// 次のようなクラス名を生成します。",
                    "// .igx-primary-500-bg { ... };",
                    "@include igx-color-classes(",
                    "  $prop: 'background-color',",
                    "  $suffix: 'bg'",
                    ");"
                ]
            }
        ]
    },
    "igx-css-vars": {
        "description": [
            "<p>範囲にテーマの色を追加します。</p>",
            ""
        ],
        "parameters": {
            "theme": {
                "description": [
                    "<p>使用されるコンポーネントのテーマ。</p>",
                    ""
                ]
            }
        }
    },
    "if-ltr": {
        "description": [
            "<p>グローバルな $direction 変数が &#39;ltr&#39; に設定されている場合にのみ、渡されたコンテンツを CSS にコンパイルします。</p>",
            ""
        ],
        "example": [
            {
                "description": [
                    "<p>$direction が ltr (デフォルト) に設定されている場合にのみコンテンツを含めます。</p>",
                    ""
                ],
                "code": [
                    "@include if-ltr {",
                    "  margin-right: 8px;",
                    "}"
                ]
            }
        ]
    },
    "if-rtl": {
        "description": [
            "<p>グローバルな $direction 変数が &#39;rtl&#39; に設定されている場合にのみ、渡されたコンテンツを CSS にコンパイルします。</p>",
            ""
        ],
        "example": [
            {
                "description": [
                    "<p>$direction が rtl に設定されている場合にのみコンテンツを含めます。</p>",
                    ""
                ],
                "code": [
                    "@include if-rtl {",
                    "  margin-left: 8px;",
                    "}"
                ]
            }
        ]
    },
    "line-clamp": {
        "description": [
            "<p>特定の行数でテキストを切り捨てます。</p>",
            ""
        ],
        "parameters": {
            "lines": {
                "description": [
                    "<p>表示する行数</p>",
                    ""
                ]
            },
            "inline": {
                "description": [
                    "<p>要素が inline-box または box を表示するかどうかを設定します。</p>",
                    ""
                ]
            },
            "vertical": {
                "description": [
                    "<p>要素がコンテンツを水平または垂直に配置するかどうかを設定します。</p>",
                    ""
                ]
            }
        },
        "example": [
            {
                "description": [
                    "<p>4 行目以降のテキストを切り捨てます。</p>",
                    ""
                ],
                "code": [
                    "@include line-clamp(4, true, true);"
                ]
            }
        ]
    }
}
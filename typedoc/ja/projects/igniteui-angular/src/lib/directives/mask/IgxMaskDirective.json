{
    "IgxMaskDirective": {
        "properties": {
            "promptChar": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Sets the character representing a fillable spot in the input mask.\nDefault value is \"'_'\".\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input [promptChar] = \"'/'\">\n```"
                        }
                    ]
                }
            },
            "includeLiterals": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Specifies if the bound value includes the formatting symbols.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input [includeLiterals] = \"true\">\n```"
                        }
                    ]
                }
            },
            "displayValuePipe": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Specifies a pipe to be used on blur.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input [displayValuePipe] = \"displayFormatPipe\">\n```"
                        }
                    ]
                }
            },
            "focusedValuePipe": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Specifies a pipe to be used on focus.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input [focusedValuePipe] = \"inputFormatPipe\">\n```"
                        }
                    ]
                }
            },
            "valueChanged": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emits an event each time the value changes.\nProvides "
                        },
                        {
                            "kind": "code",
                            "text": "`rawValue: string`"
                        },
                        {
                            "kind": "text",
                            "text": " and "
                        },
                        {
                            "kind": "code",
                            "text": "`formattedValue: string`"
                        },
                        {
                            "kind": "text",
                            "text": " as event arguments.\n"
                        },
                        {
                            "kind": "code",
                            "text": "```html\n<input (valueChanged) = \"valueChanged(rawValue: string, formattedValue: string)\">\n```"
                        }
                    ]
                }
            }
        },
        "methods": {},
        "accessors": {
            "mask": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the input mask.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```html\n<input [igxMask] = \"'00/00/0000'\">\n```"
                            }
                        ]
                    }
                }
            }
        }
    }
}
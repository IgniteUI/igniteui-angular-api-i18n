{
    "IgxTabsComponent": {
        "comment": {
            "blockTags": [
                {
                    "tag": "@igxModule",
                    "content": [
                        {
                            "kind": "text",
                            "text": "IgxTabsModule"
                        }
                    ]
                },
                {
                    "tag": "@igxTheme",
                    "content": [
                        {
                            "kind": "text",
                            "text": "igx-tabs-theme"
                        }
                    ]
                },
                {
                    "tag": "@igxKeywords",
                    "content": [
                        {
                            "kind": "text",
                            "text": "tabs"
                        }
                    ]
                },
                {
                    "tag": "@igxGroup",
                    "content": [
                        {
                            "kind": "text",
                            "text": "Layouts"
                        }
                    ]
                },
                {
                    "tag": "@remarks",
                    "content": [
                        {
                            "kind": "text",
                            "text": "The Ignite UI for Angular Tabs component places tabs at the top and allows for scrolling when there are multiple tab items on the screen."
                        }
                    ]
                },
                {
                    "tag": "@example",
                    "content": [
                        {
                            "kind": "code",
                            "text": "```html\n<igx-tabs>\n    <igx-tab-item>\n        <igx-tab-header>\n            <igx-icon igxTabHeaderIcon>folder</igx-icon>\n            <span igxTabHeaderLabel>Tab 1</span>\n        </igx-tab-header>\n        <igx-tab-content>\n            Content 1\n        </igx-tab-content>\n    </igx-tab-item>\n    ...\n</igx-tabs>\n```"
                        }
                    ]
                }
            ],
            "modifierTags": {},
            "summary": [
                {
                    "kind": "text",
                    "text": "Tabs component is used to organize or switch between similar data sets."
                }
            ]
        },
        "properties": {
            "selectedIndexChange": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Output to enable support for two-way binding on [(selectedIndex)]"
                        }
                    ]
                }
            },
            "selectedIndexChanging": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emitted when the selected index is about to change."
                        }
                    ]
                }
            },
            "selectedItemChange": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Emitted when the selected item is changed."
                        }
                    ]
                }
            },
            "items": {
                "comment": {
                    "blockTags": [],
                    "modifierTags": {},
                    "summary": [
                        {
                            "kind": "text",
                            "text": "Returns the items."
                        }
                    ]
                }
            }
        },
        "methods": {},
        "accessors": {
            "tabAlignment": {
                "getter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@Input",
                                "content": [
                                    {
                                        "kind": "text",
                                        "text": "property which determines the tab alignment. Defaults to "
                                    },
                                    {
                                        "kind": "code",
                                        "text": "`start`"
                                    },
                                    {
                                        "kind": "text",
                                        "text": "."
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "An"
                            }
                        ]
                    }
                }
            },
            "selectedIndex": {
                "getter": {
                    "comment": {
                        "blockTags": [
                            {
                                "tag": "@Input",
                                "content": [
                                    {
                                        "kind": "text",
                                        "text": "property that gets/sets the index of the selected item.\nDefault value is 0 if contents are defined otherwise defaults to -1."
                                    }
                                ]
                            }
                        ],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "An"
                            }
                        ]
                    }
                }
            },
            "disableAnimation": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Enables/disables the transition animation of the contents."
                            }
                        ]
                    }
                }
            },
            "selectedItem": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Gets the selected item."
                            }
                        ]
                    }
                }
            }
        }
    }
}
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "An accessor that sets the resource strings.\nBy default it uses EN resources."
                            }
                        ]
                    }
                }
            },
            "weekStart": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Gets the start day of the week.\nCan return a numeric or an enum representation of the week day.\nIf not set, defaults to the first day of the week for the application locale."
                            }
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets the start day of the week.\nCan be assigned to a numeric value or to "
                            },
                            {
                                "kind": "code",
                                "text": "`WEEKDAYS`"
                            },
                            {
                                "kind": "text",
                                "text": " enum value."
                            }
                        ]
                    }
                }
            },
            "locale": {
                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Gets the "
                            },
                            {
                                "kind": "code",
                                "text": "`locale`"
                            },
                            {
                                "kind": "text",
                                "text": " of the calendar.\nIf not set, defaults to application's locale."

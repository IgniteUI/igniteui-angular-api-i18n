                "getter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Returns whether the "
                            },
                            {
                                "kind": "code",
                                "text": "`IgxNavbarComponent`"
                            },
                            {
                                "kind": "text",
                                "text": " action button is visible, true/false.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```typescript\n @ViewChild(\"MyChild\")\npublic navBar: IgxNavbarComponent;\nngAfterViewInit(){\n    let actionButtonVisibile = this.navBar.isActionButtonVisible;\n}\n```"
                            }
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "blockTags": [],
                        "modifierTags": {},
                        "summary": [
                            {
                                "kind": "text",
                                "text": "Sets whether the action button of the "
                            },
                            {
                                "kind": "code",
                                "text": "`IgxNavbarComponent`"
                            },
                            {
                                "kind": "text",
                                "text": " is visible.\n"
                            },
                            {
                                "kind": "code",
                                "text": "```html\n<igx-navbar [title]=\"currentView\" [isActionButtonVisible]=\"'false'\"></igx-navbar>\n```"
                            }
                        ]
                    }
                }
            }
        }
    }
}
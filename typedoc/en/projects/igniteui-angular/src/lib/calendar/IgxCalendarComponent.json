{
    "IgxCalendarComponent": {
        "comment": {
            "tags": {
                "igxmodule": {
                    "comment": {
                        "text": "IgxCalendarModule",
                        "tagName": "igxmodule"
                    }
                },
                "igxtheme": {
                    "comment": {
                        "text": "igx-calendar-theme, igx-icon-theme",
                        "tagName": "igxtheme"
                    }
                },
                "igxkeywords": {
                    "comment": {
                        "text": "calendar, datepicker, schedule, date",
                        "tagName": "igxkeywords"
                    }
                },
                "igxgroup": {
                    "comment": {
                        "text": "Scheduling",
                        "tagName": "igxgroup"
                    }
                },
                "remarks": {
                    "comment": {
                        "text": [
                            "The Ignite UI Calendar provides an easy way to display a calendar and allow users to select dates using single, multiple",
                            "or range selection."
                        ],
                        "tagName": "remarks"
                    }
                },
                "example:": {
                    "comment": {
                        "text": [
                            "```html",
                            "<igx-calendar selection=\"range\"></igx-calendar>",
                            "```"
                        ],
                        "tagName": "example:"
                    }
                }
            },
            "shortText": "Calendar provides a way to display date information."
        },
        "properties": {
            "id": {
                "comment": {
                    "tags": {
                        "remarks": {
                            "comment": {
                                "text": "If not set, the `id` will have value `\"igx-calendar-0\"`.",
                                "tagName": "remarks"
                            }
                        },
                        "example": {
                            "comment": {
                                "text": [
                                    "```html",
                                    "<igx-calendar id=\"my-first-calendar\"></igx-calendar>",
                                    "```"
                                ],
                                "tagName": "example"
                            }
                        },
                        "memberof": {
                            "comment": {
                                "text": "IgxCalendarComponent",
                                "tagName": "memberof"
                            }
                        }
                    },
                    "shortText": "Sets/gets the `id` of the calendar."
                }
            },
            "hasHeader": {
                "comment": {
                    "tags": {
                        "example": {
                            "comment": {
                                "text": [
                                    "```html",
                                    "<igx-calendar [hasHeader]=\"false\"></igx-calendar>",
                                    "```"
                                ],
                                "tagName": "example"
                            }
                        }
                    },
                    "shortText": [
                        "Sets/gets whether the calendar has header.",
                        "Default value is `true`."
                    ]
                }
            },
            "vertical": {
                "comment": {
                    "tags": {
                        "example": {
                            "comment": {
                                "text": [
                                    "```html",
                                    "<igx-calendar [vertical] = \"true\"></igx-calendar>",
                                    "```"
                                ],
                                "tagName": "example"
                            }
                        }
                    },
                    "shortText": [
                        "Sets/gets whether the calendar header will be in vertical position.",
                        "Default value is `false`."
                    ]
                }
            },
            "showWeekNumbers": {
                "comment": {
                    "tags": {
                        "example": {
                            "comment": {
                                "text": [
                                    "```html",
                                    "<igx-calendar [showWeekNumbers]=\"true\"></igx-calendar>",
                                    "``"
                                ],
                                "tagName": "example"
                            }
                        }
                    },
                    "shortText": "Show/hide week numbers"
                }
            },
            "activeViewIdx": {
                "comment": {
                    "tags": {},
                    "shortText": "Holds month view index we are operating on."
                }
            },
            "hideOutsideDays": {
                "comment": {
                    "tags": {},
                    "shortText": [
                        "Sets/gets whether the outside dates (dates that are out of the current month) will be hidden.",
                        "Default value is `false`.",
                        "```html",
                        "<igx-calendar [hideOutsideDays] = \"true\"></igx-calendar>",
                        "```",
                        "```typescript",
                        "let hideOutsideDays = this.calendar.hideOutsideDays;",
                        "```"
                    ]
                }
            },
            "selected": {
                "comment": {
                    "tags": {},
                    "shortText": [
                        "Emits an event when a date is selected.",
                        "Provides reference the `selectedDates` property."
                    ]
                }
            },
            "viewDateChanged": {
                "comment": {
                    "tags": {},
                    "shortText": [
                        "Emits an event when the month in view is changed.",
                        "```html",
                        "<igx-calendar (viewDateChanged)=\"viewDateChanged($event)\"></igx-calendar>",
                        "```",
                        "```typescript",
                        "public viewDateChanged(event: IViewDateChangeEventArgs) {",
                        " let viewDate = event.currentValue;",
                        "}",
                        "```"
                    ]
                }
            },
            "activeViewChanged": {
                "comment": {
                    "tags": {},
                    "shortText": [
                        "Emits an event when the active view is changed.",
                        "```html",
                        "<igx-calendar (activeViewChanged)=\"activeViewChanged($event)\"></igx-calendar>",
                        "```",
                        "```typescript",
                        "public activeViewChanged(event: CalendarView) {",
                        " let activeView = event;",
                        "}",
                        "```"
                    ]
                }
            }
        },
        "methods": {
            "deselectDate": {
                "comment": {
                    "tags": {
                        "example": {
                            "comment": {
                                "text": [
                                    "```typescript",
                                    " this.calendar.deselectDate(new Date(`2018-06-12`));",
                                    "````"
                                ],
                                "tagName": "example"
                            }
                        }
                    },
                    "shortText": "Deselects date(s) (based on the selection type)."
                }
            },
            "selectDate": {
                "comment": {
                    "tags": {},
                    "shortText": "Selects date(s) (based on the selection type)."
                }
            }
        },
        "accessors": {
            "monthsViewNumber": {
                "getter": {
                    "comment": {
                        "tags": {
                            "example": {
                                "comment": {
                                    "text": [
                                        "```html",
                                        "<igx-calendar [monthsViewNumber]=\"2\"></igx-calendar>",
                                        "```"
                                    ],
                                    "tagName": "example"
                                }
                            }
                        },
                        "shortText": [
                            "Sets/gets the number of month views displayed.",
                            "Default value is `1`."
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Sets/gets the number of month views displayed.",
                            "Default value is `1`."
                        ]
                    }
                }
            },
            "headerTemplate": {
                "getter": {
                    "comment": {
                        "tags": {
                            "example": {
                                "comment": {
                                    "text": [
                                        "```typescript",
                                        "let headerTemplate =  this.calendar.headerTeamplate;",
                                        "```"
                                    ],
                                    "tagName": "example"
                                }
                            },
                            "memberof": {
                                "comment": {
                                    "text": "IgxCalendarComponent",
                                    "tagName": "memberof"
                                }
                            }
                        },
                        "shortText": "Gets the header template."
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {
                            "example": {
                                "comment": {
                                    "text": [
                                        "```html",
                                        "<igx-calendar headerTemplateDirective = \"igxCalendarHeader\"></igx-calendar>",
                                        "```"
                                    ],
                                    "tagName": "example"
                                }
                            },
                            "memberof": {
                                "comment": {
                                    "text": "IgxCalendarComponent",
                                    "tagName": "memberof"
                                }
                            }
                        },
                        "shortText": "Sets the header template."
                    }
                }
            },
            "subheaderTemplate": {
                "getter": {
                    "comment": {
                        "tags": {
                            "example": {
                                "comment": {
                                    "text": [
                                        "```typescript",
                                        "let subheaderTemplate = this.calendar.subheaderTemplate;",
                                        "```"
                                    ],
                                    "tagName": "example"
                                }
                            }
                        },
                        "shortText": "Gets the subheader template."
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {
                            "example": {
                                "comment": {
                                    "text": [
                                        "```html",
                                        "<igx-calendar subheaderTemplate = \"igxCalendarSubheader\"></igx-calendar>",
                                        "```"
                                    ],
                                    "tagName": "example"
                                }
                            },
                            "memberof": {
                                "comment": {
                                    "text": "IgxCalendarComponent",
                                    "tagName": "memberof"
                                }
                            }
                        },
                        "shortText": "Sets the subheader template."
                    }
                }
            },
            "headerContext": {
                "getter": {
                    "comment": {
                        "tags": {
                            "example": {
                                "comment": {
                                    "text": [
                                        "```typescript",
                                        "let headerContext =  this.calendar.headerContext;",
                                        "```"
                                    ],
                                    "tagName": "example"
                                }
                            }
                        },
                        "shortText": "Gets the context for the template marked with the `igxCalendarHeader` directive."
                    }
                }
            },
            "context": {
                "getter": {
                    "comment": {
                        "tags": {
                            "example": {
                                "comment": {
                                    "text": [
                                        "```typescript",
                                        "let context =  this.calendar.context;",
                                        "```"
                                    ],
                                    "tagName": "example"
                                }
                            }
                        },
                        "shortText": [
                            "Gets the context for the template marked with either `igxCalendarSubHeaderMonth`",
                            "or `igxCalendarSubHeaderYear` directive."
                        ]
                    }
                }
            },
            "activeView": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Gets the current active view.",
                            "```typescript",
                            "this.activeView = calendar.activeView;",
                            "```"
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Sets the current active view.",
                            "```html",
                            "<igx-calendar [activeView]=\"year\" #calendar></igx-calendar>",
                            "```",
                            "```typescript",
                            "calendar.activeView = IgxCalendarView.YEAR;",
                            "```"
                        ]
                    }
                }
            },
            "resourceStrings": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": "An accessor that returns the resource strings."
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "An accessor that sets the resource strings.",
                            "By default it uses EN resources."
                        ]
                    }
                }
            },
            "weekStart": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Gets the start day of the week.",
                            "Can return a numeric or an enum representation of the week day.",
                            "Defaults to `Sunday` / `0`."
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Sets the start day of the week.",
                            "Can be assigned to a numeric value or to `WEEKDAYS` enum value."
                        ]
                    }
                }
            },
            "locale": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Gets the `locale` of the calendar.",
                            "Default value is `\"en\"`."
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Sets the `locale` of the calendar.",
                            "Expects a valid BCP 47 language tag.",
                            "Default value is `\"en\"`."
                        ]
                    }
                }
            },
            "formatOptions": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": "Gets the date format options of the days view."
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Sets the date format options of the days view.",
                            "Default is { day: 'numeric', month: 'short', weekday: 'short', year: 'numeric' }"
                        ]
                    }
                }
            },
            "formatViews": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Gets whether the `day`, `month` and `year` should be rendered",
                            "according to the locale and formatOptions, if any."
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Gets whether the `day`, `month` and `year` should be rendered",
                            "according to the locale and formatOptions, if any."
                        ]
                    }
                }
            },
            "selection": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Gets the selection type.",
                            "Default value is `\"single\"`.",
                            "Changing the type of selection resets the currently",
                            "selected values if any."
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": "Sets the selection."
                    }
                }
            },
            "value": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "text": [
                            "When selection is set to `single`, it returns",
                            "a single `Date` object.",
                            "Otherwise it is an array of `Date` objects."
                        ],
                        "shortText": "Gets the selected date(s)."
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "text": [
                            "When selection is set to `single`, it accepts",
                            "a single `Date` object.",
                            "Otherwise it is an array of `Date` objects."
                        ],
                        "shortText": "Sets the selected date(s)."
                    }
                }
            },
            "viewDate": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Gets the date that is presented.",
                            "By default it is the current date."
                        ]
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": "Sets the date that will be presented in the default view when the component renders."
                    }
                }
            },
            "disabledDates": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": "Gets the disabled dates descriptors."
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Sets the disabled dates' descriptors.",
                            "```typescript",
                            "@ViewChild(\"MyCalendar\")",
                            "public calendar: IgxCalendarComponent;",
                            "ngOnInit(){",
                            "   this.calendar.disabledDates = [",
                            "    {type: DateRangeType.Between, dateRange: [new Date(\"2020-1-1\"), new Date(\"2020-1-15\")]},",
                            "    {type: DateRangeType.Weekends}];",
                            "}",
                            "```"
                        ]
                    }
                }
            },
            "specialDates": {
                "getter": {
                    "comment": {
                        "tags": {},
                        "shortText": "Gets the special dates descriptors."
                    }
                },
                "setter": {
                    "comment": {
                        "tags": {},
                        "shortText": [
                            "Sets the special dates' descriptors.",
                            "```typescript",
                            "@ViewChild(\"MyCalendar\")",
                            "public calendar: IgxCalendarComponent;",
                            "ngOnInit(){",
                            "   this.calendar.specialDates = [",
                            "    {type: DateRangeType.Between, dateRange: [new Date(\"2020-1-1\"), new Date(\"2020-1-15\")]},",
                            "    {type: DateRangeType.Weekends}];",
                            "}",
                            "```"
                        ]
                    }
                }
            }
        }
    }
}